  "system_id": "gps_system",
  "system_name": "GPS System",
  "specification": {
    "designScenario": {
      "name": "Advanced Integrated Navigation System (AINS)",
      "description": "A next-generation navigation system combining multi-constellation GNSS, hybrid inertial navigation, and AI-driven predictive routing for optimal flight efficiency and safety. Incorporates real-time space weather monitoring, terrain awareness, and cyber-resilient architecture to address modern aviation challenges in congested airspace and evolving threat environments.",
      "requirements": [
        {
          "title": "Multi-Source Navigation Fusion",
          "type": "functional",
          "details": [
            {
              "description": "Integrate GPS (L1/L5), Galileo (E1/E5), and GLONASS (G1/G3) signals with inertial reference system and celestial navigation backup"
            },
            {
              "description": "Implement sensor fusion algorithms combining ADS-B, DME, and VOR data for ground-based verification"
            }
          ]
        },
        {
          "title": "High-Precision Navigation Performance",
          "type": "performance",
          "details": [
            {
              "description": "Maintain 3D position accuracy of ±2.5 meters (95% CEP) under ionospheric disturbance conditions"
            },
            {
              "description": "Achieve 50 Hz update rate for attitude determination with <5ms latency from sensor input to output"
            }
          ]
        },
        {
          "title": "Cybersecurity and Fault Tolerance",
          "type": "safety",
          "details": [
            {
              "description": "Implement quantum-resistant encryption for all GNSS signal processing chains"
            },
            {
              "description": "Quadruple redundant navigation computers with dissimilar architecture (x86/ARM/RISC-V)"
            }
          ]
        },
        {
          "title": "Operational Adaptability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Support in-flight reconfiguration for alternative navigation during GNSS denial scenarios"
            },
            {
              "description": "Provide 3D weather radar integration with navigation path optimization"
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Multi-Constellation GNSS Receiver",
