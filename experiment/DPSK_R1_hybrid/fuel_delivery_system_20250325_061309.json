        "description": "Emergency fuel dump capacity with dual redundant dump valves"
      },
      {
        "name": "vaporPressure",
        "type": "string",
        "value": "7-49 kPa @ 20°C",
        "description": "Maintained fuel vapor pressure envelope to prevent cavitation or vapor lock"
      },
      {
        "name": "filtrationLevel",
        "type": "string",
        "value": "5 micron absolute",
        "description": "Multi-stage filtration with coalescer elements meeting SAE ARP9013A"
      }
    ]
