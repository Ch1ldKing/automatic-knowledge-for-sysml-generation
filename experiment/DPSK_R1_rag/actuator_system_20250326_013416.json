{
  "system_id": "actuator_system",
  "system_name": "Actuator System",
  "specification": {
    "designScenario": {
      "name": "Actuators",
      "description": "The Actuators system is a hybrid electro-hydraulic and electro-mechanical module responsible for precise control of flight surfaces, landing gear deployment/retraction, and braking systems. It combines hydraulic reliability with electric redundancy to meet modern aviation safety standards. The system integrates adaptive control algorithms, self-diagnostic capabilities, and fault-tolerant architectures to ensure operational integrity under extreme aerodynamic loads, temperature variations, and partial system failures.",
      "requirements": [
        {
          "title": "Hybrid Actuation",
          "type": "functional",
          "details": [
            {
              "description": "Provide dual-mode actuation (hydraulic primary, electric backup) for flight control surfaces to ensure continuous operation during hydraulic system degradation."
            }
          ]
        },
        {
          "title": "Dynamic Load Adaptation",
          "type": "performance",
          "details": [
            {
              "description": "Adjust actuation force output (15-25 kN) in real-time based on aerodynamic load sensors to prevent surface flutter while maintaining ±0.1° positional accuracy."
            }
          ]
        },
        {
          "title": "Redundant Control Pathways",
          "type": "safety",
          "details": [
            {
              "description": "Implement triple-redundant control signal processing with voting logic to isolate and bypass faulty actuator commands within 50ms."
            }
          ]
        },
        {
          "title": "Condition-Based Maintenance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Embed vibration analysis and fluid particulate sensors to predict bearing wear and seal degradation, enabling maintenance intervals up to 1,200 flight hours."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Electro-Hydrostatic Actuator (EHA)",
        "type": "Primary Actuation Unit",
        "description": "Self-contained hydraulic actuator with local electric pump for redundant operation, eliminating centralized hydraulic dependency for critical flight surfaces.",
        "subComponents": [
          {
            "name": "Variable Displacement Pump",
            "description": "Electronically controlled swashplate pump regulating fluid flow based on flight control computer demands",
            "type": "Hydraulic Component"
          },
          {
            "name": "Solenoid Override Valve",
            "description": "Fail-safe valve isolating actuator from central hydraulic system during EHA mode activation",
            "type": "Control Valve"
          }
        ]
      },
      {
        "name": "Electro-Mechanical Actuator (EMA)",
        "type": "Backup Actuation System",
        "description": "All-electric secondary actuation system with harmonic drive gearing for high-torque applications in flight control surfaces.",
        "subComponents": [
          {
            "name": "Torque Limiter",
            "description": "Mechanical slip clutch preventing jamming scenarios by decoupling motor during overload conditions",
            "type": "Safety Mechanism"
          }
        ]
      },
      {
        "name": "Actuator Control Unit (ACU)",
        "type": "Embedded Controller",
        "description": "Triple-channel FPGA-based controller implementing MIL-STD-1553B interfaces for command processing and health monitoring.",
        "subComponents": null
      }
    ],
    "relationships": [
      {
        "source": "Flight Control Computer",
        "target": "Actuators",
        "relationshipType": "association",
        "description": "Transmits surface deflection commands via redundant ARINC 825 networks to actuator control units"
      },
      {
        "source": "Actuators",
        "target": "Central Hydraulic System",
        "relationshipType": "dependency",
        "description": "Primary hydraulic power source for normal operation mode, with automatic isolation during backup activation"
      },
      {
        "source": "Actuators",
        "target": "EMA",
        "relationshipType": "composition",
        "description": "EMA units are integral subcomponents providing backup actuation capacity within the Actuators module"
      },
      {
        "source": "Actuators",
        "target": "Aircraft Health Management System",
        "relationshipType": "aggregation",
        "description": "Streams actuator performance metrics and prognostic data for fleet-wide maintenance optimization"
      }
    ],
    "attributes": [
      {
        "name": "forceDynamicRange",
        "type": "string",
        "value": "15-25 kN",
        "description": "Adjustable force output range accommodating varying aerodynamic loads during different flight regimes"
      },
      {
        "name": "transitionLatency",
        "type": "string",
        "value": "<100ms",
        "description": "Maximum allowable delay when switching between hydraulic primary and electric backup actuation modes"
      },
      {
        "name": "shockTolerance",
        "type": "string",
        "value": "50g, 11ms duration",
        "description": "Mechanical resilience to sudden impact loads during hard landings or turbulence"
      },
      {
        "name": "powerDensity",
        "type": "string",
        "value": "2.4 kW/kg",
        "description": "Power-to-weight ratio for EMA subsystems, critical for minimizing parasitic mass in flight surfaces"
      },
      {
        "name": "EMIRating",
        "type": "string",
        "value": "DO-160G Section 21",
        "description": "Electromagnetic interference immunity level ensuring operation in high-radiation environments"
      }
    ]
  },
