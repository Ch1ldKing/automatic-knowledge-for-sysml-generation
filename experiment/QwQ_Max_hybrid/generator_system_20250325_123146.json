      },
      {
        "name": "thermalProtectionThreshold",
        "type": "string",
        "value": "150°C",
        "description": "Maximum winding temperature at which the generator initiates thermal shutdown."
      },
      {
        "name": "complianceStandards",
        "type": "string",
        "value": "MIL-STD-704F, RTCA DO-160G",
        "description": "Aviation standards the generator system is certified against for environmental and electrical performance."
      }
    ]
  },
