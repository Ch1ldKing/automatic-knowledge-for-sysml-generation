{
  "system_id": "generator_system",
  "system_name": "Generator System",
  "specification": {
    "designScenario": {
      "name": "Generator System",
      "description": "Design a high-reliability Generator System for aircraft applications, responsible for converting mechanical energy from engines into regulated AC and DC electrical power, ensuring uninterrupted power generation under varying flight conditions and engine speeds. The system must interface with the Electrical Power System to supply conditioned power to avionics, flight controls, and cabin systems.",
      "requirements": [
        {
          "title": "Power Generation & Distribution",
          "type": "functional",
          "details": [
            {
              "description": "Generate AC power at 115V 400Hz compliant with MIL-STD-704 standards for aircraft systems."
            },
            {
              "description": "Include rectifiers and inverters to provide isolated DC and variable AC outputs for diverse subsystems."
            },
            {
              "description": "Ensure seamless power transfer between multiple generators during engine start or failure."
            }
          ]
        },
        {
          "title": "Performance Under Load Variations",
          "type": "performance",
          "details": [
            {
              "description": "Maintain output voltage stability within ±2% during engine speed fluctuations from idle to maximum RPM."
            },
            {
              "description": "Achieve transient response recovery time of less than 200ms for sudden load changes mimicking rapid actuation of control surfaces."
            }
          ]
        },
        {
          "title": "Safety and Protection",
          "type": "safety",
          "details": [
            {
              "description": "Implement over-speed protection to prevent mechanical failure during engine overspeed conditions."
            },
            {
              "description": "Include thermal protection mechanisms to shut down the generator in case of winding overheating."
            }
          ]
        },
        {
          "title": "Environmental and Design Standards",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Adhere to MIL-STD-810G for vibration and shock resistance in engine nacelle environments."
            },
            {
              "description": "Ensure electromagnetic compatibility (EMC) with avionics to avoid signal interference."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Engine-Driven Generator",
        "type": "Power Generation Unit",
        "description": "Primary component converting mechanical rotational energy from aircraft engines into electrical power via electromagnetic induction.",
        "subComponents": [
          {
            "name": "Stator Assembly",
            "type": "Electrical Component",
            "description": "Stationary coil assembly generating alternating magnetic fields to induce voltage."
          },
          {
            "name": "Rotor Assembly",
            "type": "Mechanical Component",
            "description": "Rotating armature driven by the engine, housing field coils for magnetic flux generation."
          },
          {
            "name": "Exciter Module",
            "type": "Electrical Component",
            "description": "Provides regulated DC excitation current to the rotor field coils for voltage control."
          }
        ]
      },
      {
        "name": "Voltage Regulation Module",
        "type": "Control Module",
        "description": "Regulates output voltage stability and adjusts excitation current based on load conditions.",
        "subComponents": [
          {
            "name": "Voltage Regulator",
            "type": "Electronic Controller",
            "description": "Microprocessor-based controller maintaining output voltage within specified tolerances."
          },
          {
            "name": "Current Sensor Array",
            "type": "Sensing Device",
            "description": "Monitors output current to detect overloads and phase imbalances."
          }
        ]
      },
      {
        "name": "Frequency Stabilization Unit",
        "type": "Control Module",
        "description": "Maintains 400Hz output frequency despite engine speed variations through phase-locked loop technology.",
        "subComponents": null
      },
      {
        "name": "Power Conversion Hub",
        "type": "Distribution Module",
        "description": "Manages conversion between AC and DC outputs using solid-state inverters and rectifiers.",
        "subComponents": [
          {
            "name": "DC Output Rectifier",
            "type": "Power Electronics",
            "description": "Converts 115V AC to regulated DC outputs for avionics."
          },
          {
            "name": "Isolation Transformer",
            "type": "Electrical Component",
            "description": "Ensures galvanic isolation between generator windings and aircraft power bus."
          }
        ]
      },
      {
        "name": "Generator Control Unit (GCU)",
        "type": "Control Module",
        "description": "Manages generator startup, shutdown, and fault diagnostics via digital interface.",
        "subComponents": [
          {
            "name": "Digital Signal Processor (DSP)",
            "type": "Electronic Component",
            "description": "Processes real-time sensor data for closed-loop control."
          },
          {
            "name": "Fault Logic Board",
            "type": "Electronic Component",
            "description": "Implements protection algorithms for fault isolation and shutdown."
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Engine-Driven Generator",
        "target": "Voltage Regulation Module",
        "relationshipType": "dependency",
        "description": "The Voltage Regulation Module adjusts excitation current based on generator output feedback."
      },
      {
        "source": "Frequency Stabilization Unit",
        "target": "Engine-Driven Generator",
        "relationshipType": "dependency",
        "description": "The Frequency Stabilization Unit compensates for engine speed variations to maintain output frequency."
      },
      {
        "source": "Generator Control Unit (GCU)",
        "target": "Power Conversion Hub",
        "relationshipType": "aggregation",
        "description": "The GCU monitors and controls the Power Conversion Hub's output parameters."
      },
      {
        "source": "Power Conversion Hub",
        "target": "Voltage Regulation Module",
        "relationshipType": "association",
        "description": "The Power Conversion Hub receives regulated voltage from the Voltage Regulation Module."
      },
      {
        "source": "Generator Control Unit (GCU)",
        "target": "Frequency Stabilization Unit",
        "relationshipType": "aggregation",
        "description": "The GCU integrates frequency stabilization data into overall system diagnostics."
      }
    ],
    "attributes": [
      {
        "name": "ratedPowerOutput",
        "type": "string",
        "value": "90kVA",
        "description": "Maximum continuous power output capacity of the generator under standard operating conditions."
      },
      {
        "name": "outputFrequency",
