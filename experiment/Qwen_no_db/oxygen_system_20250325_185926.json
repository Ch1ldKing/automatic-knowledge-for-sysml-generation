{
  "system_id": "oxygen_system",
  "system_name": "Oxygen System",
  "specification": {
    "designScenario": {
      "name": "Oxygen Supply and Regulation System",
      "description": "This design outlines an Oxygen System intended for aviation applications, integrated within the Environmental Control System (ECS). The system ensures continuous delivery of breathable air at safe oxygen concentrations, maintains pressure stability, and provides emergency oxygen supply during cabin depressurization. It employs redundant oxygen sources, precise monitoring, and automated control to adhere to aviation safety standards.",
      "requirements": [
        {
          "title": "Oxygen Concentration Regulation",
          "type": "functional",
          "details": [
            {
              "description": "Maintain cabin oxygen concentration between 19.5% and 23.5% by volume using oxygen generators and distribution systems."
            }
          ]
        },
        {
          "title": "Pressure Stability",
          "type": "functional",
          "details": [
            {
              "description": "Ensure oxygen delivery pressure matches cabin pressure to prevent leakage and maintain consistent supply."
            }
          ]
        },
        {
          "title": "Emergency Oxygen Provision",
          "type": "functional",
          "details": [
            {
              "description": "Automatically deploy emergency oxygen masks and activate auxiliary oxygen sources within 2 seconds of cabin depressurization."
            }
          ]
        },
        {
          "title": "Response Time to Oxygen Depletion",
          "type": "performance",
          "details": [
            {
              "description": "The system shall detect oxygen levels below 19.5% and initiate corrective actions within 3 seconds."
            }
          ]
        },
        {
          "title": "Redundancy and Fail-Safety",
          "type": "safety",
          "details": [
            {
              "description": "Implement dual oxygen generation units and independent distribution pathways to ensure uninterrupted supply during single-point failures."
            }
          ]
        },
        {
          "title": "Modular Design",
          "type": "nonFunctional",
          "details": [
            {
              "description": "All components must support rapid replacement and maintenance in accordance with FAA-approved modular standards."
            }
          ]
        },
        {
          "title": "User Interface Integration",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Provide real-time oxygen level displays and fault diagnostics on the ECS control panel."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Oxygen Generation Unit",
        "type": "Subsystem",
        "description": "Generates and regulates oxygen supply using onboard sources or bleed air.",
        "subComponents": [
          {
            "name": "Oxygen Concentrator",
            "description": "Extracts and purifies oxygen from cabin air via pressure swing adsorption technology.",
            "type": "Mechanical Device"
          },
          {
            "name": "Pressure Regulator",
            "description": "Maintains consistent oxygen delivery pressure matching cabin pressure requirements.",
            "type": "Valve Assembly"
          },
          {
            "name": "Filter Module",
            "description": "Removes particulates and contaminants from oxygen supply.",
            "type": "Filter"
          }
        ]
      },
      {
        "name": "Distribution Network",
        "type": "Subsystem",
        "description": "Distributes oxygen to cabin zones via ducts and outlets.",
        "subComponents": [
          {
            "name": "Oxygen Ducts",
            "description": "High-pressure conduits for oxygen transport.",
            "type": "Conduit"
          },
          {
            "name": "Outlet Valves",
            "description": "Regulates oxygen flow to individual passenger and crew zones.",
            "type": "Actuator"
          }
        ]
      },
      {
        "name": "Oxygen Monitoring System",
        "type": "Subsystem",
        "description": "Continuously measures oxygen levels and triggers alarms for deviations.",
        "subComponents": [
          {
            "name": "Oxygen Sensor Array",
            "description": "Electrochemical sensors distributed across cabin zones.",
            "type": "Sensor"
          },
          {
            "name": "Alarm Module",
            "description": "Audible/visual alerts for low oxygen levels and system faults.",
            "type": "Indicator"
          }
        ]
      },
      {
        "name": "Emergency Oxygen Supply",
        "type": "Subsystem",
        "description": "Stores and deploys emergency oxygen masks and auxiliary oxygen canisters.",
        "subComponents": [
          {
            "name": "Emergency Canister Bank",
            "description": "High-pressure oxygen canisters for rapid deployment.",
            "type": "Storage Unit"
          },
          {
            "name": "Mask Deployment Mechanism",
            "description": "Automatically releases oxygen masks from overhead panels.",
            "type": "Actuator"
          }
        ]
      },
      {
        "name": "Control Unit Interface",
        "type": "Electronic Module",
        "description": "Manages oxygen system operations and interfaces with the ECS control panel.",
        "subComponents": [
          {
            "name": "Oxygen Control Module",
            "description": "Processes sensor data and sends control signals to actuators.",
            "type": "Controller"
          },
          {
            "name": "Status Display",
            "description": "Graphical interface showing real-time oxygen parameters.",
            "type": "Display Unit"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Oxygen Generation Unit",
        "target": "Distribution Network",
        "relationshipType": "aggregation",
        "description": "The Oxygen Generation Unit provides oxygen to the Distribution Network for cabin delivery."
      },
      {
        "source": "Oxygen Monitoring System",
