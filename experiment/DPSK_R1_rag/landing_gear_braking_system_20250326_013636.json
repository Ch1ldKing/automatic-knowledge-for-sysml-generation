{
  "system_id": "landing_gear_braking_system",
  "system_name": "Landing Gear Braking System",
  "specification": {
    "designScenario": {
      "name": "Landing Gear Braking System",
      "description": "The Landing Gear Braking System is a critical subsystem responsible for decelerating the aircraft during landing, taxiing, and aborted takeoffs. It integrates friction-based braking, hydraulic actuation, and anti-skid control to ensure safe and reliable deceleration across diverse runway conditions. The system is designed to withstand extreme thermal loads, provide redundancy for fail-safe operation, and maintain compatibility with aircraft weight distribution and avionics interfaces.",
      "requirements": [
        {
          "title": "Functional Deceleration Control",
          "type": "functional",
          "details": [
            {
              "description": "The system must apply controlled braking force to all landing gear wheels proportionally to pilot input and aircraft weight distribution."
            },
            {
              "description": "Integrate with anti-skid systems to prevent wheel lockup during low-friction runway conditions."
            }
          ]
        },
        {
          "title": "Thermal and Mechanical Performance",
          "type": "performance",
          "details": [
            {
              "description": "Dissipate 2.5 MW of thermal energy during maximum effort braking without structural degradation."
            },
            {
              "description": "Achieve full braking torque within 0.3 seconds of hydraulic pressure application."
            }
          ]
        },
        {
          "title": "Operational Safety",
          "type": "safety",
          "details": [
            {
              "description": "Maintain braking capability with 50% hydraulic system failure through redundant fluid circuits."
            },
            {
              "description": "Implement automatic brake temperature monitoring with cockpit alerts for critical thermal thresholds."
            }
          ]
        },
        {
          "title": "Maintenance and Certification",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Enable brake pad replacement within 90 minutes per wheel assembly using standard ground service equipment."
            },
            {
              "description": "Comply with EASA CS-25 and FAA FAR Part 25 certification requirements for braking systems."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Brake Actuators",
        "description": "Electro-hydraulic units converting fluid pressure into mechanical clamping force on brake discs.",
        "type": "Actuation Subsystem",
        "subComponents": [
