        "relationshipType": "association",
        "description": "Sensors in the Oxygen Monitoring System send data to the Control Unit Interface for analysis."
      },
      {
        "source": "Emergency Oxygen Supply",
        "target": "Control Unit Interface",
        "relationshipType": "dependency",
        "description": "The Emergency Oxygen Supply activates based on commands from the Control Unit Interface during depressurization."
      },
      {
        "source": "Oxygen Generation Unit",
        "target": "Oxygen Concentrator",
        "relationshipType": "composition",
        "description": "The Oxygen Concentrator is a core component of the Oxygen Generation Unit."
      },
      {
        "source": "Distribution Network",
        "target": "Outlet Valves",
        "relationshipType": "aggregation",
        "description": "Outlet Valves are integrated into the Distribution Network to regulate zone-specific oxygen flow."
      }
    ],
