{
  "system_id": "reservoir_system",
  "system_name": "Reservoir System",
  "specification": {
    "designScenario": {
      "name": "Aircraft Hydraulic Fluid Reservoir System",
      "description": "The Fluid Reservoir System is designed to store, manage, and supply hydraulic fluid under extreme environmental and operational conditions. It incorporates advanced thermal insulation, real-time monitoring, and integrated filtration to ensure consistent fluid quality and pressure. The system supports high-pressure hydraulic actuation systems while maintaining lightweight and durable construction for prolonged service intervals.",
      "requirements": [
        {
          "title": "Fluid Storage and Thermal Management",
          "type": "functional",
          "details": [
            {
              "description": "The system must store hydraulic fluid within a temperature range of -40°C to 85°C while maintaining structural integrity."
            },
            {
              "description": "Supply fluid at a maximum flow rate of 200 liters per minute to meet peak system demands."
            }
          ]
        },
        {
          "title": "Pressure Regulation and Stability",
          "type": "performance",
          "details": [
            {
              "description": "Maintain fluid pressure between 2000 and 4500 psi during all operational phases without exceeding ±5% deviation."
            }
          ]
        },
        {
          "title": "Contamination and Leak Prevention",
          "type": "safety",
          "details": [
            {
              "description": "The High-Efficiency Filter must remove 99.9% of particulates larger than 5 microns to protect downstream components."
            },
            {
              "description": "The Leak Detection System must trigger an alarm within 30 seconds of detecting a leak exceeding 0.1 mL/min."
            }
          ]
        },
        {
          "title": "Durability and Weight Optimization",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Constructed from titanium alloy to minimize weight while ensuring corrosion resistance and fatigue strength."
            },
            {
              "description": "Support a service interval of 500 flight hours with routine checks and full maintenance every 2000 flight hours."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Fluid Reservoir System",
        "description": "Central storage and management unit for hydraulic fluid, ensuring consistent supply under varying operational conditions.",
        "type": "Storage Unit",
        "subComponents": [
          {
            "name": "Reservoir Tank",
            "description": "Primary container for storing hydraulic fluid with integrated thermal and pressure management features.",
            "type": "Container",
            "subComponents": [
              {
                "name": "Thermal Insulation Layer",
                "description": "Multi-layer insulation to maintain fluid temperature stability during extreme environmental conditions.",
                "type": "Insulation"
              },
              {
                "name": "Level Sensor",
                "description": "Electronic sensor to monitor fluid level and trigger refilling alerts.",
                "type": "Sensor"
              }
            ]
          },
          {
            "name": "Filtration and Pressure Regulation Unit",
            "description": "Integrated system for filtering contaminants and maintaining optimal fluid pressure.",
            "type": "Filter",
            "subComponents": [
              {
                "name": "High-Efficiency Filter",
                "description": "Micron-level filter to remove particulates and prevent contamination.",
                "type": "Filter"
              },
              {
                "name": "Pressure Regulator",
                "description": "Mechanical regulator to adjust fluid pressure within operational limits.",
                "type": "Regulator"
              }
            ]
          },
          {
            "name": "Leak Detection System",
            "description": "Pressure-sensitive sensors and alarms to detect fluid leaks in real-time.",
            "type": "Sensor"
          }
        ]
      }
    ],
