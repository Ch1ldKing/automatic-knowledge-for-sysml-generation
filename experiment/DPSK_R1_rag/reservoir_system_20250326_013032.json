{
  "system_id": "reservoir_system",
  "system_name": "Reservoir System",
  "specification": {
    "designScenario": {
      "name": "Fluid Reservoir",
      "description": "The Fluid Reservoir is a mission-critical component in modern aircraft hydraulic systems, engineered to store, condition, and distribute hydraulic fluid under extreme operational conditions. It features advanced thermal management, real-time fluid quality monitoring, and adaptive pressure regulation to support next-generation fly-by-wire systems, thrust vectoring controls, and high-cycle actuation systems. The reservoir maintains fluid integrity across -65°F to 275°F operational envelopes while withstanding 9g maneuver loads.",
      "requirements": [
        {
          "title": "Dynamic Fluid Management",
          "type": "functional",
          "details": [
            {
              "description": "The reservoir shall automatically adjust fluid volume compensation for thermal expansion/contraction and altitude-induced pressure changes through active bladder systems."
            }
          ]
        },
        {
          "title": "Adaptive Pressure Regulation",
          "type": "performance",
          "details": [
            {
              "description": "Maintain system pressure within ±2% of setpoint across 800-5500 psi operational range, with <100ms response time to transient demands from flight control actuators."
            }
          ]
        },
        {
          "title": "Containment Integrity",
          "type": "safety",
          "details": [
            {
              "description": "Triple-redundant burst protection with fail-safe isolation valves to prevent catastrophic fluid loss during overpressure events exceeding 125% MAWP (Maximum Allowable Working Pressure)."
            }
          ]
        },
        {
          "title": "Condition-Based Maintenance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Integrated prognostic health monitoring system capable of predicting filter life, seal degradation, and fluid quality with 95% accuracy using multi-parameter sensor fusion."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Primary Fluid Chamber",
        "type": "Pressure Vessel",
        "description": "Monolithic composite structure with titanium reinforcement rings, designed for 2:1 burst-to-yield ratio and corrosion resistance to MIL-H-5606/83282 fluids",
        "subComponents": [
          {
            "name": "Variable Geometry Bladder",
            "description": "Fluorosilicone elastomer expansion membrane with shape memory alloy actuators for active volume control",
            "type": "Compensation System"
          },
          {
            "name": "Cryogenic Baffle Array",
            "description": "Phase-change material heat exchangers for thermal stabilization during high-altitude operations",
            "type": "Thermal Management"
          }
        ]
      },
      {
        "name": "Fluid Conditioning Module",
        "type": "Filtration System",
        "description": "Triple-redundant parallel filtration train with 3μm absolute particulate removal and electrostatic deaerators",
        "subComponents": [
          {
            "name": "Chip Detector Array",
            "description": "Multi-spectral ferrous/non-ferrous particle sensors with real-time debris analysis",
            "type": "Condition Monitoring"
          },
          {
            "name": "Dielectric Moisture Trap",
            "description": "Hydroscopic molecular sieve maintaining fluid water content <200ppm",
            "type": "Fluid Treatment"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Primary Fluid Chamber",
        "target": "Variable Geometry Bladder",
        "relationshipType": "composition",
        "description": "The bladder system is integrally mounted within the primary chamber's expansion volume cavity"
      },
      {
        "source": "Fluid Conditioning Module",
        "target": "Hydraulic Pump Inlet",
        "relationshipType": "aggregation",
        "description": "Conditioned fluid output is directly coupled to the main hydraulic pump suction port via hardline interface"
      },
      {
        "source": "Flight Control Computer",
        "target": "Chip Detector Array",
        "relationshipType": "dependency",
        "description": "Particle count data feeds into the aircraft's central maintenance system for component life prediction"
      }
    ],
    "attributes": [
      {
        "name": "thermalOperatingRange",
        "type": "string",
        "value": "-65°F to +275°F",
        "description": "Full operational temperature spectrum with maintained viscosity control"
      },
      {
        "name": "contaminantLevel",
        "type": "string",
        "value": "NAS Class 5",
        "description": "Maximum allowable particulate contamination per AS4059 standards"
      },
      {
        "name": "materialComposition",
        "type": "string",
        "value": "Ti-6Al-4V/CFRP hybrid",
        "description": "Titanium-composite layered construction for optimal strength-to-weight ratio"
      },
      {
        "name": "diagnosticInterval",
        "type": "string",
        "value": "Continuous PHM streaming",
        "description": "Real-time prognostic health monitoring with 50ms sampling rate"
      }
    ]
  },
