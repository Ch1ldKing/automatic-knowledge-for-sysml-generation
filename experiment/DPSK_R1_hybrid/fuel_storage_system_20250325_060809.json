{
  "system_id": "fuel_storage_system",
  "system_name": "Fuel Storage System",
  "specification": {
    "designScenario": {
      "name": "Fuel Storage System",
      "description": "A critical subsystem responsible for the safe storage, containment, and management of aviation fuel. It ensures structural integrity, leak prevention, and thermal stability of fuel under extreme altitude variations, temperature fluctuations, and vibration loads. Designed with aerospace-grade materials and redundant safety features to meet FAA/EASA certification requirements.",
      "requirements": [
        {
          "title": "Fuel Containment Integrity",
          "type": "functional",
          "details": [
            {
              "description": "The system must maintain complete fuel containment without leaks during all flight phases including maximum turbulence loads (up to 5g) and temperature ranges (-50°C to +70°C)."
            }
          ]
        },
        {
          "title": "Structural Load Capacity",
          "type": "performance",
          "details": [
            {
              "description": "Tank structure must withstand 200% of maximum expected operational pressure (MEOP) with safety factor of 3.0 against yield failure."
            }
          ]
        },
        {
          "title": "Explosion Prevention",
          "type": "safety",
          "details": [
            {
              "description": "Incorporate inerting gas distribution and flame arrestors to maintain oxygen concentration below 12% in ullage space per FAA AC 25.981-2."
            }
          ]
        },
        {
          "title": "Corrosion Resistance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "All wetted surfaces shall demonstrate 10,000-hour salt spray resistance without pitting corrosion per ASTM B117 standards."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Primary Fuel Tank",
        "description": "Main fuel reservoir with integral baffle system to prevent fuel sloshing, constructed from AA7075-T6 aluminum alloy with elastomeric sealing.",
        "type": "Pressure Vessel",
        "subComponents": [
          {
            "name": "Baffle System",
            "description": "Anti-slosh partitions with check valves for controlled fuel migration during maneuvers",
            "type": "Structural Component"
          },
          {
            "name": "Access Panel",
            "description": "Hermetically sealed inspection port with lightning-safe design",
            "type": "Maintenance Interface"
          }
        ]
      },
      {
        "name": "Venting System",
        "description": "Dual-channel vent network maintaining atmospheric pressure differential <0.25 psi during climb/descent",
        "type": "Pressure Management",
        "subComponents": [
          {
            "name": "Pressure Relief Valve",
            "description": "Spring-loaded valve activating at 1.5x MEOP",
            "type": "Safety Component"
          },
          {
            "name": "Flame Arrestor",
            "description": "Stainless steel mesh preventing external ignition sources",
            "type": "Fire Protection"
          }
        ]
      },
      {
        "name": "Fuel Quantity Indicating System (FQIS)",
        "description": "Capacitive probe array with temperature compensation for ±1% fuel volume accuracy",
        "type": "Measurement System",
        "subComponents": [
          {
            "name": "Probe Array",
            "description": "Multi-segment capacitance sensors with anti-icing heaters",
            "type": "Sensor"
          },
          {
            "name": "Compensator Unit",
            "description": "Real-time dielectric constant adjustment based on fuel temperature",
            "type": "Signal Processor"
          }
        ]
      },
      {
        "name": "Refuel/Defuel Manifold",
        "description": "Dual-path fueling system with automatic shutoff at 98% capacity",
        "type": "Fluid Handling",
        "subComponents": [
          {
            "name": "Pressure Actuated Valve",
            "description": "Quick-disconnect compatible refueling receptor",
            "type": "Flow Control"
          },
          {
            "name": "Fuel Defuel Pump",
            "description": "15 l/sec transfer capacity for emergency fuel jettison",
            "type": "Actuator"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Primary Fuel Tank",
        "target": "Venting System",
        "relationshipType": "composition",
        "description": "The venting system is an integral structural component of the primary fuel tank assembly"
      },
      {
        "source": "FQIS",
