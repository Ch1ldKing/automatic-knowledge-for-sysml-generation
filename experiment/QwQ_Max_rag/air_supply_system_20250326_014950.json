{
  "system_id": "air_supply_system",
  "system_name": "Air Supply System",
  "specification": {
    "designScenario": {
      "name": "Air Supply System",
      "description": "The Air Supply System is designed to provide a continuous supply of pressurized, filtered, and conditioned air to the aircraft cabin. It integrates advanced airflow management, filtration, and distribution technologies to ensure passenger safety, comfort, and adherence to aviation standards. The system is engineered to handle variable flight conditions, including altitude changes, while maintaining air purity and circulation. It incorporates redundant components and real-time monitoring to ensure fault tolerance and rapid response to deviations.",
      "requirements": [
        {
          "title": "Airflow Supply",
          "type": "functional",
          "details": [
            {
              "description": "The system shall deliver a minimum airflow of 5000 CFM to the cabin under nominal flight conditions to ensure adequate ventilation."
            }
          ]
        },
        {
          "title": "Air Purity",
          "type": "functional",
          "details": [
            {
              "description": "The system must filter particulates, allergens, and contaminants using HEPA and activated carbon filters to meet FAA air quality standards."
            }
          ]
        },
        {
          "title": "Pressure Maintenance",
          "type": "functional",
          "details": [
            {
              "description": "Maintain cabin pressure within 0.8 atm to 1.2 atm during all flight phases using pressure regulators and bleed air systems."
            }
          ]
        },
        {
          "title": "Response Time",
          "type": "performance",
          "details": [
            {
              "description": "The system must respond to pressure drops or airflow blockages within 2 seconds to prevent cabin depressurization."
            }
          ]
        },
        {
          "title": "Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "Implement dual redundant compressors and filtration paths to ensure continuous air supply during single-component failure."
            }
          ]
        },
        {
          "title": "Maintenance Accessibility",
          "type": "nonFunctional",
          "details": [
            {
              "description": "All components shall be modular and accessible via standardized access panels for rapid servicing during ground maintenance."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Air Compressor Module",
        "description": "Core component responsible for pressurizing bleed air from the engines and distributing it to cabin systems.",
        "type": "Mechanical Subsystem",
        "subComponents": [
          {
            "name": "Compressor Motor",
            "description": "Variable-speed motor driven by engine bleed air to regulate airflow pressure.",
            "type": "Actuator"
          },
          {
            "name": "Pressure Regulator",
            "description": "Controls outlet pressure to maintain safe cabin pressure levels.",
            "type": "Valve Assembly"
          }
        ]
      },
      {
        "name": "Filter Bank Assembly",
        "description": "Multi-stage filtration unit removing particulates, microbes, and gaseous contaminants from cabin air.",
        "type": "Environmental Control Subsystem",
        "subComponents": [
          {
            "name": "HEPA Filter",
            "description": "High-efficiency particulate air filter capturing 99.97% of particles ≥0.3 µm.",
            "type": "Filter"
          },
          {
            "name": "Activated Carbon Filter",
            "description": "Adsorbs volatile organic compounds and odors from cabin air.",
            "type": "Filter"
          }
        ]
      },
      {
        "name": "Distribution Network",
        "description": "Network of ducts and valves directing conditioned air to cabin zones while maintaining pressure equilibrium.",
        "type": "Aerospace Conduit System",
        "subComponents": [
          {
            "name": "Main Duct Assembly",
            "description": "Primary airflow pathway constructed from fire-resistant composite materials.",
            "type": "Conduit"
          },
          {
            "name": "Zone Control Valves",
            "description": "Automated dampers regulating airflow distribution to passenger and crew compartments.",
            "type": "Valve Assembly"
          }
        ]
      },
      {
        "name": "Oxygen Supply Unit",
        "description": "Emergency oxygen delivery system with independent supply for rapid deployment during cabin depressurization.",
        "type": "Safety Subsystem",
        "subComponents": [
          {
            "name": "Oxygen Canisters",
            "description": "High-pressure oxygen storage cylinders meeting FAA capacity requirements.",
            "type": "Storage Container"
          },
          {
            "name": "Deployment Mechanism",
            "description": "Pyrotechnic-actuated valve system releasing oxygen masks automatically.",
            "type": "Safety Device"
          }
        ]
      },
      {
        "name": "Air Quality Monitor",
        "description": "Sensor array continuously measuring oxygen levels, CO2 concentration, and particulate counts.",
        "type": "Monitoring Module",
        "subComponents": [
          {
            "name": "Oxygen Sensor",
            "description": "Electrochemical sensor ensuring cabin oxygen levels remain ≥19.5%.",
            "type": "Sensor"
          },
          {
            "name": "CO2 Analyzer",
            "description": "Non-dispersive infrared sensor tracking CO2 concentrations below 1000 ppm.",
            "type": "Sensor"
          }
        ]
      }
    ],
    "relationships": [
      {
