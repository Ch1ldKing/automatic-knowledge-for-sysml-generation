{
  "system_id": "pressurization_system",
  "system_name": "Pressurization System",
  "specification": {
    "designScenario": {
      "name": "Pressurization System",
      "description": "The Pressurization System maintains safe and comfortable cabin pressure levels by regulating airflow and pressure differentials during all flight phases. It dynamically adjusts cabin altitude to simulate near-sea-level conditions (typically ≤8,000 ft) despite external atmospheric changes. The system integrates with the Environmental Control System (ECS) to manage outflow valves, monitor pressure sensors, and activate safety mechanisms during rapid decompression events. It ensures structural integrity of the airframe while prioritizing passenger comfort and compliance with aviation safety regulations.",
      "requirements": [
        {
          "title": "Cabin Pressure Regulation",
          "type": "functional",
          "details": [
            {
              "description": "Automatically maintain cabin pressure between 0.8 atm and 1.2 atm using closed-loop control of outflow valves and air supply inputs."
            }
          ]
        },
        {
          "title": "Altitude Rate Limiting",
          "type": "functional",
          "details": [
            {
              "description": "Limit cabin altitude change rates to ≤500 ft/min during ascent/descent to prevent passenger discomfort."
            }
          ]
        },
        {
          "title": "Pressure Differential Safety",
          "type": "safety",
          "details": [
            {
              "description": "Implement redundant pressure relief valves to prevent cabin-to-external pressure differentials exceeding 9.0 psi (airframe limit)."
            }
          ]
        },
        {
          "title": "Decompression Response",
          "type": "performance",
          "details": [
            {
              "description": "Detect rapid decompression events and initiate corrective actions (oxygen mask deployment, emergency descent) within 2 seconds."
            }
          ]
        },
        {
          "title": "Sensor Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "Provide triple-redundant pressure sensor arrays with voting logic to prevent single-point failures."
            }
          ]
        },
        {
          "title": "Maintenance Diagnostics",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Include built-in test equipment (BITE) for valve actuation tests and leak rate calculations during ground maintenance."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Pressure Control Unit",
        "description": "Central controller processing sensor data and executing pressurization schedules via PID algorithms.",
        "type": "Avionics Computer",
        "subComponents": [
          {
            "name": "Flight Phase Scheduler",
            "description": "Generates target pressure schedules based on aircraft altitude and flight phase (climb, cruise, descent).",
            "type": "Software Module"
          },
          {
            "name": "Valve Actuator Controller",
            "description": "Converts pressure error signals into precise valve position commands.",
            "type": "Control Hardware"
          }
        ]
      },
      {
        "name": "Outflow Valve Assembly",
        "description": "Electro-mechanically actuated valve modulating cabin exhaust airflow to control pressure.",
        "type": "Mechanical Subsystem",
        "subComponents": [
          {
            "name": "Valve Actuator",
            "description": "Electric motor or pneumatic actuator providing torque for valve positioning.",
            "type": "Actuator"
          },
          {
            "name": "Valve Position Sensor",
            "description": "LVDT or rotary encoder providing real-time valve angle feedback.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Cabin Pressure Sensors",
        "description": "Redundant absolute pressure transducers measuring cabin altitude equivalent pressure.",
        "type": "Sensor Array",
        "subComponents": [
          {
            "name": "Primary Pressure Transducer",
            "description": "High-accuracy piezoresistive sensor with temperature compensation.",
            "type": "Sensor"
          },
          {
            "name": "Backup Capacitive Sensor",
            "description": "Secondary measurement using diaphragm capacitance technique.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Safety Relief Valves",
        "description": "Passive mechanical valves opening automatically at preset differential pressures.",
        "type": "Safety Device",
        "subComponents": null
      },
      {
        "name": "Air Supply Duct",
        "description": "Bleed air conduit delivering compressed air from engines/APU for pressurization.",
        "type": "Pneumatic Conduit",
        "subComponents": null
      }
    ],
    "relationships": [
      {
        "source": "Pressure Control Unit",
        "target": "Cabin Pressure Sensors",
        "relationshipType": "aggregation",
        "description": "Control unit aggregates data from all cabin pressure sensors for differential voting"
      },
      {
        "source": "Pressure Control Unit",
        "target": "Outflow Valve Assembly",
        "relationshipType": "dependency",
        "description": "Valve positioning commands from control unit directly drive actuator movements"
      },
      {
        "source": "Safety Relief Valves",
        "target": "Outflow Valve Assembly",
        "relationshipType": "composition",
        "description": "Safety valves are physically integrated into outflow valve housing as fail-safe mechanism"
      },
      {
        "source": "Air Supply Duct",
        "target": "Pressure Control Unit",
        "relationshipType": "association",
        "description": "Control unit monitors bleed air supply pressure via dedicated transducers in duct"
      },
      {
        "source": "Pressure Control Unit",
        "target": "Environmental Control System",
        "relationshipType": "association",
        "description": "Integrated with ECS for coordinated temperature/pressure management through shared data bus"
      }
    ],
