{
  "system_id": "satellite_communication_system",
  "system_name": "Satellite Communication System",
  "specification": {
    "designScenario": {
      "name": "S A T C O M Module",
      "description": "The S A T C O M Module is an advanced avionics system enabling global, secure, and low-latency satellite communication for passenger aircraft. It supports real-time telemetry, flight path optimization, crew/passenger connectivity, and emergency coordination. The module employs adaptive frequency agility, multi-layer encryption, and fault-tolerant architecture to meet DO-178C/DAL-B safety standards and ARINC 791 interoperability requirements.",
      "requirements": [
        {
          "title": "Multi-Band Satellite Connectivity",
          "type": "functional",
          "details": [
            {
              "description": "Establish simultaneous links with L-band (1-2 GHz) and Ka-band (26.5-40 GHz) satellites for global coverage and high-throughput data transfer."
            },
            {
              "description": "Automatically switch between satellite constellations (e.g., Iridium, Inmarsat, ViaSat) based on signal strength and network availability."
            }
          ]
        },
        {
          "title": "High-Speed Data Performance",
          "type": "performance",
          "details": [
            {
              "description": "Achieve asymmetric data rates up to 50 Mbps downlink / 10 Mbps uplink in Ka-band mode for passenger connectivity services."
            },
            {
              "description": "Maintain end-to-end latency below 300ms for ATC datalink communications (CPDLC) during critical flight phases."
            }
          ]
        },
        {
          "title": "Fault-Tolerant Operation",
          "type": "safety",
          "details": [
            {
              "description": "Implement triple-redundant modem units with majority voting logic to prevent single-point communication failures."
            },
            {
              "description": "Integrate radiation-hardened components capable of withstanding single-event upsets (SEUs) at maximum cruise altitudes."
            }
          ]
        },
        {
          "title": "Cybersecurity & Maintainability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Utilize quantum-resistant encryption (CRYSTALS-Kyber) with FIPS 140-3 compliant key management for classified data channels."
            },
            {
              "description": "Support over-the-air (OTA) software updates with dual-bank flash memory to enable in-service upgrades without downtime."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "S A T C O M Module",
        "description": "Primary communication hub integrating satellite transceivers, beamforming antennas, and network security components for airborne connectivity",
        "type": "Avionics Data Communication Unit",
        "subComponents": [
          {
            "name": "Phased-Array Antenna System",
            "description": "Electronically steerable antenna array with 256 elements providing ±75° azimuth/elevation coverage and adaptive beamforming",
            "type": "RF Subsystem"
          },
          {
            "name": "Multi-Band Transceiver Stack",
            "description": "Software-defined radio supporting simultaneous L-band (Aero-L) and Ka-band (SwiftBroadband-Safety) waveforms with 4x4 MIMO capability",
            "type": "Digital Signal Processor"
          },
          {
            "name": "Quantum Encryption Unit",
            "description": "Post-quantum cryptography module implementing lattice-based algorithms for key exchange and AES-GCM-256 data encryption",
            "type": "Security Hardware"
          },
          {
            "name": "Network Processor Unit",
            "description": "Dual-core ARM Cortex-A76 processor managing QoS for multiple virtual channels (ATN, ACARS, APD, passenger Wi-Fi)",
            "type": "Data Routing Controller"
          },
          {
            "name": "Power Conditioning Module",
            "description": "28VDC-to-48VDC converter with surge protection and 90% efficiency under MIL-STD-704F power quality standards",
            "type": "Power Electronics"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "S A T C O M Module",
        "target": "Phased-Array Antenna System",
        "relationshipType": "composition",
        "description": "The antenna system is an integral part of the SATCOM Module, physically enclosed within the LRU and managed via dedicated beam steering controllers"
      },
      {
        "source": "Multi-Band Transceiver Stack",
        "target": "Quantum Encryption Unit",
        "relationshipType": "association",
        "description": "All transceiver I/O channels are routed through the encryption unit for mandatory data-in-transit protection"
      },
      {
        "source": "Network Processor Unit",
        "target": "Central Integrated Network",
        "relationshipType": "dependency",
        "description": "The processor depends on the aircraft's central network for time synchronization via ARINC 664 P7 datalinks"
      },
      {
        "source": "Power Conditioning Module",
        "target": "Essential Power Bus",
        "relationshipType": "aggregation",
        "description": "Draws power from both main and emergency electrical buses with automatic load shedding capabilities"
      }
    ],
    "attributes": [
      {
