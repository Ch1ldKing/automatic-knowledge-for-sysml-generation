{
  "system_id": "pump_system",
  "system_name": "Pump System",
  "specification": {
    "designScenario": {
      "name": "Hydraulic Pump",
      "description": "The Hydraulic Pump serves as the primary energy conversion unit in an aircraft's hydraulic system, efficiently converting mechanical energy into hydraulic pressure with enhanced energy efficiency and thermal management. It powers control surfaces, landing gears, and braking systems while operating under extreme conditions. The design incorporates dual-stage pump technology, real-time pressure and temperature monitoring, and full integration with aircraft avionics for enhanced diagnostics and redundancy.",
      "requirements": [
        {
          "title": "Energy Conversion Efficiency",
          "type": "functional",
          "details": [
            {
              "description": "The pump shall achieve an energy conversion efficiency of at least 90% under nominal operating conditions to minimize energy losses."
            }
          ]
        },
        {
          "title": "Pressure Generation",
          "type": "performance",
          "details": [
            {
              "description": "The pump must maintain an operating pressure between 1800 psi and 4800 psi with deviations not exceeding ±10% during steady-state operations."
            }
          ]
        },
        {
          "title": "Rapid Actuation",
          "type": "performance",
          "details": [
            {
              "description": "The system shall achieve target hydraulic pressure within 0.4 seconds to support high-speed actuation demands during critical flight phases."
            }
          ]
        },
        {
          "title": "Redundant Fail-Safe Operation",
          "type": "safety",
          "details": [
            {
              "description": "The pump must include dual fail-safe mechanisms to reroute hydraulic fluid to a standby reservoir and isolate faulty sections in case of malfunction."
            }
          ]
        },
        {
          "title": "Thermal Management and Durability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "The pump shall incorporate a thermal management system to maintain operational temperatures between -40°C and 120°C, with a maintenance cycle of 500 flight hours."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Hydraulic Pump",
        "type": "Power Unit",
        "description": "Primary energy conversion unit utilizing dual-stage pump technology to convert mechanical energy into hydraulic pressure, supporting aircraft control systems with enhanced efficiency and redundancy.",
        "subComponents": [
          {
            "name": "Drive Motor",
            "type": "Electric Motor",
            "description": "Electric motor providing variable-speed mechanical input to the pump assembly for optimized energy efficiency."
          },
          {
            "name": "Pump Assembly",
            "type": "Dual-Stage Actuator",
            "description": "Mechanical assembly with two-stage pressurization to ensure consistent hydraulic output under varying load conditions."
          },
          {
            "name": "Temperature Control Module",
            "type": "Thermal Unit",
            "description": "Integrated thermal management system monitoring and regulating operational temperatures to prevent overheating."
          },
          {
            "name": "Pressure Sensor",
            "type": "Sensor",
            "description": "Real-time pressure monitoring unit ensuring accurate pressure regulation and fault detection."
          }
        ]
      },
      {
        "name": "Fluid Reservoir",
        "type": "Storage Unit",
        "description": "Stores and supplies aerospace-grade hydraulic fluid, maintaining cleanliness and adequate fluid levels for the pump's operation.",
        "subComponents": [
          {
            "name": "Reservoir Tank",
            "type": "Container",
            "description": "Primary storage container for hydraulic fluid with integrated filtration and pressure regulation."
          },
          {
            "name": "Filtration Unit",
            "type": "Filter",
            "description": "Removes contaminants from hydraulic fluid to ensure system reliability and component longevity."
          },
          {
            "name": "Pressure Regulator",
            "type": "Valve",
            "description": "Regulates fluid pressure entering the Hydraulic Pump to maintain optimal operational parameters."
          }
        ]
      },
      {
        "name": "Hydraulic Valve",
        "type": "Control Unit",
        "description": "Manages fluid flow direction and pressure distribution to various aircraft systems based on real-time operational demands.",
        "subComponents": null
      }
    ],
