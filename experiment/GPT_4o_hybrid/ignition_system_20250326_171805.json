      "description": "The ignition system is a critical component within the aircraft designed to ignite the fuel-air mixture in the engine cylinders, ensuring reliable engine startup and operation across various conditions.",
      "name": "Ignition System",
      "requirements": [
        {
          "details": [
            {
              "description": "The system must reliably produce and deliver a spark at the appropriate timing for efficient engine operation."
            }
          ],
          "title": "Spark Generation",
          "type": "functional"
        },
        {
          "details": [
            {
              "description": "The system should operate with high efficiency and quick response times under standard and emergency conditions."
            }
          ],
          "title": "Performance Consistency",
          "type": "performance"
        },
        {
          "details": [
            {
              "description": "Include safeguards to manage electrical faults and prevent fire or spark hazards."
            }
          ],
          "title": "Electrical Safety",
          "type": "safety"
        },
        {
          "details": [
            {
              "description": "The system should be easy to install and resilient to wear, reducing maintenance overhead."
            }
          ],
          "title": "Maintainability",
          "type": "nonFunctional"
        }
      ]
    },
    "relationships": [
      {
        "description": "The Spark Plug is dependent on the Ignition Coil for high voltage supply.",
        "relationshipType": "dependency",
        "source": "Spark Plug",
        "target": "Ignition Coil"
      },
      {
        "description": "The Ignition Control Unit aggregates signals from the Engine Sensors to regulate timing.",
        "relationshipType": "aggregation",
        "source": "Ignition Control Unit",
        "target": "Engine Sensors"
      }
    ],
    "systemComponents": [
      {
        "description": "Converts low-voltage power to high voltage necessary for spark generation.",
        "name": "Ignition Coil",
        "subComponents": [
          {
            "description": "Conducts and modulates current for inductive increase.",
            "name": "Primary Coil",
            "type": "Electrical"
          },
          {
            "description": "Supplies the elevated voltage to the distributor.",
            "name": "Secondary Coil",
            "type": "Electrical"
          }
        ],
        "type": "Electric Component"
      },
      {
        "description": "Generates a spark to ignite the air-fuel mixture within the combustion chamber.",
        "name": "Spark Plug",
        "subComponents": [
          {
            "description": "Conducts high voltage to the electrode tip.",
            "name": "Electrode Assembly",
            "type": "Conductive Element"
          },
          {
            "description": "Isolates electrical current to focus the discharge at the tip.",
            "name": "Insulator",
            "type": "Insulation"
          }
        ],
        "type": "Mechanical Component"
      },
      {
        "description": "Controls ignition timing and processes inputs from sensors and pilot commands.",
        "name": "Ignition Control Unit",
        "subComponents": [
          {
            "description": "Processes input data to adjust spark timings effectively.",
            "name": "Processor Module",
            "type": "Logic Circuitry"
          },
          {
            "description": "Enables communication and adjustments by maintenance personnel.",
            "name": "Diagnostic Interface",
            "type": "Monitoring"
          }
        ],
        "type": "Control System"
      },
      {
        "description": "Monitors engine states like temperature and crankshaft position to optimize ignition.",
        "name": "Engine Sensors",
        "subComponents": [
          {
            "description": "Detects crankshaft position for precise spark timing.",
            "name": "Crankshaft Sensor",
            "type": "Sensor"
          },
          {
            "description": "Determines engine operating temperature.",
            "name": "Temperature Sensor",
            "type": "Sensor"
          }
        ],
        "type": "Monitoring System"
      }
    ]
  },
