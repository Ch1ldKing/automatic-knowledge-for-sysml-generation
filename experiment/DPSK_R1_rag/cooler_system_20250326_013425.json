  "system_name": "Cooler System",
  "specification": {
    "designScenario": {
      "name": "Advanced Cabin Cooler System",
      "description": "The Advanced Cabin Cooler System is a next-generation environmental control subsystem designed to maintain optimal cabin temperature under extreme external conditions. It employs a variable-speed refrigeration cycle with eco-friendly refrigerants, adaptive airflow management, and predictive thermal load balancing. The system integrates smart sensors, hybrid heat exchangers, and redundant power pathways to ensure rapid cooling response, energy efficiency, and fault tolerance. It supports dynamic adjustments based on cabin occupancy, altitude variations, and external climate data while maintaining compatibility with modern aircraft power architectures.",
      "requirements": [
        {
          "title": "Adaptive Cooling Control",
          "type": "functional",
          "details": [
            {
              "description": "The cooler shall dynamically adjust cooling output based on real-time cabin occupancy data, external temperature, and altitude-derived pressure changes."
            }
          ]
        },
        {
          "title": "Energy-Efficient Operation",
          "type": "performance",
          "details": [
            {
              "description": "The system shall maintain a coefficient of performance (COP) ≥ 3.5 across all operational altitudes between sea level and 45,000 ft."
            }
          ]
        },
        {
          "title": "Emergency Cooling Reserve",
          "type": "safety",
          "details": [
            {
              "description": "Dual independent cooling circuits with N+2 redundancy shall maintain minimum cooling capacity during single or dual component failures."
            }
          ]
        },
        {
          "title": "Acoustic Compliance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Operational noise shall not exceed 65 dB(A) at passenger ear level during maximum cooling load conditions."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Smart Cooling Unit",
        "type": "Subsystem",
        "description": "Primary cooling assembly combining refrigeration cycle components with intelligent flow control and predictive load management",
        "subComponents": [
          {
            "name": "Variable-Speed Magnetic Compressor",
            "description": "Oil-free compressor with wide modulation range (10-100% capacity) for precise cooling output control",
            "type": "Actuator"
          },
          {
            "name": "Hybrid Microchannel Heat Exchanger",
            "description": "Combined condenser/evaporator unit with phase-change material integration for thermal inertia mitigation",
            "type": "Heat Exchanger"
          },
          {
            "name": "Smart Refrigerant Flow Regulator",
            "description": "Electro-mechanical valve array with predictive mass flow adjustment based on cabin thermal dynamics",
            "type": "Control Element"
          },
          {
            "name": "Multi-Zone Air Ducting",
            "description": "Pressure-optimized airflow distribution network with individually controllable venturis for cabin sectors",
            "type": "Duct System"
          }
        ]
      },
      {
        "name": "Thermal Sensing Array",
        "type": "Sensor",
        "description": "Distributed network of hyperspectral thermal sensors with 3D cabin temperature mapping capability",
        "subComponents": [
          {
            "name": "Multi-Point IR Sensors",
            "description": "Infrared sensor clusters measuring surface temperatures of cabin fixtures and passenger heat signatures",
            "type": "Sensor"
          },
          {
            "name": "Airflow Velocity Probes",
            "description": "Pitot-static tubes with temperature compensation for real-time convection pattern analysis",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Cooling Management Unit",
        "type": "Controller",
        "description": "Dual-channel FADEC (Full Authority Digital Environmental Control) system with machine learning-based thermal optimization",
        "subComponents": [
          {
            "name": "Predictive Load Calculator",
            "description": "Neural network module forecasting cooling demands based on flight phase and passenger load",
            "type": "Processor"
          },
          {
            "name": "Redundant Control Processors",
            "description": "Triple modular redundant computing nodes for fault-tolerant operation",
            "type": "Controller"
          }
        ]
      }
    ],
