{
  "system_id": "fuel_delivery_system",
  "system_name": "Fuel Delivery System",
  "specification": {
    "designScenario": {
      "name": "Advanced Aeronautical Fuel System",
      "description": "The Advanced Aeronautical Fuel System is designed to optimize fuel storage, distribution, and management for long-haul and high-altitude aircraft. It integrates cryogenic fuel compatibility, real-time load balancing, and predictive maintenance features to enhance efficiency, safety, and operational flexibility. The system employs advanced materials and digital controls to ensure seamless fuel delivery under extreme temperatures, high G-forces, and varying flight profiles.",
      "requirements": [
        {
          "title": "Fuel Management Optimization",
          "type": "functional",
          "details": [
            {
              "description": "The system must dynamically adjust fuel distribution between wing and center tanks to maintain optimal aircraft balance during all flight phases."
            },
            {
              "description": "Enable crossfeed functionality to transfer fuel between tanks to compensate for engine failures or uneven consumption."
            }
          ]
        },
        {
          "title": "High-Pressure Fuel Delivery",
          "type": "performance",
          "details": [
            {
              "description": "Fuel pumps must deliver a minimum flow rate of 1200 liters/minute at pressures up to 6 Bar during maximum engine demand."
            },
            {
              "description": "Maintain fuel temperature within operational limits (-40°C to +60°C) using active cooling/heating systems."
            }
          ]
        },
        {
          "title": "Cryogenic Fuel Safety",
          "type": "safety",
          "details": [
            {
              "description": "Implement triple-redundant leak detection for liquid natural gas (LNG) storage tanks with automatic isolation of compromised sections."
            },
            {
              "description": "Include fail-safe valves to prevent over-pressurization during rapid altitude changes."
            }
          ]
        },
        {
          "title": "Predictive Maintenance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Provide real-time health monitoring of all components, with automated diagnostics and remaining useful life predictions for critical parts."
            },
            {
              "description": "Ensure a mean time between failures (MTBF) exceeding 10,000 flight hours under normal operating conditions."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Cryogenic Fuel Tank",
        "description": "Primary storage container for liquid natural gas (LNG), featuring vacuum-insulated walls and composite liners to maintain cryogenic temperatures.",
        "type": "Container",
        "subComponents": [
          {
            "name": "Cryogenic Insulation Layer",
            "description": "Multi-layer insulation system with vacuum jacket to prevent heat ingress.",
            "type": "Insulation"
          },
          {
            "name": "Pressure Transducer",
            "description": "High-precision sensor monitoring internal tank pressure down to 0.1 Bar.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Variable Displacement Pump",
        "description": "Electrically driven pump with adaptive flow control to match engine demand across all flight regimes.",
        "type": "Actuator",
        "subComponents": [
          {
            "name": "Brushless Motor",
            "description": "High-torque motor with integrated thermal protection for continuous operation.",
            "type": "Motor"
          },
          {
            "name": "Flow Meter",
            "description": "Ultrasonic sensor measuring fuel flow rate in real time.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Fuel Management Computer",
        "description": "Central control unit processing sensor data, executing fuel distribution algorithms, and interfacing with flight management systems.",
        "type": "Electronic Module",
        "subComponents": [
          {
            "name": "AI Processor",
            "description": "Dedicated chip for predictive maintenance and load optimization calculations.",
            "type": "Processor"
          },
          {
            "name": "ARINC 429 Interface",
            "description": "Avionics data bus for communication with aircraft systems.",
            "type": "Interface Module"
          }
        ]
      },
      {
        "name": "Crossfeed Valve Assembly",
        "description": "Mechanical valve system enabling fuel transfer between left/right wing tanks and center tank.",
        "type": "Control Component",
        "subComponents": [
          {
            "name": "Solenoid Valve",
            "description": "Electrically actuated valve with positional feedback sensors.",
            "type": "Valve"
          },
          {
            "name": "Emergency Shutoff Mechanism",
            "description": "Mechanical backup to close valves in case of electrical failure.",
            "type": "Safety Mechanism"
          }
        ]
      },
      {
        "name": "Cryogenic Filter",
        "description": "Filtration unit removing particulates and ice crystals from LNG before entering engine systems.",
        "type": "Filter",
        "subComponents": null
      },
      {
        "name": "Leak Detection System",
        "description": "Network of pressure and ultrasonic sensors monitoring fuel lines for breaches.",
        "type": "Monitoring",
        "subComponents": [
          {
            "name": "Ultrasonic Sensor Array",
            "description": "Non-invasive sensors detecting fuel vapor leaks.",
            "type": "Sensor"
          },
          {
            "name": "Alarm Annunciator",
            "description": "Visual/auditory alert system for cockpit crew.",
            "type": "Indicator"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Variable Displacement Pump",
        "target": "Cryogenic Fuel Tank",
        "relationshipType": "dependency",
        "description": "The Pump requires fuel from the Cryogenic Fuel Tank to operate; tank failure disrupts pump functionality."
      },
      {
        "source": "Fuel Management Computer",
        "target": "Leak Detection System",
        "relationshipType": "aggregation",
        "description": "The Computer aggregates leak detection data from sensors to trigger safety protocols."
      },
      {
        "source": "Fuel Management Computer",
        "target": "Crossfeed Valve Assembly",
        "relationshipType": "association",
        "description": "The Computer controls valve actuation based on fuel distribution algorithms."
      },
      {
        "source": "Crossfeed Valve Assembly",
        "target": "Cryogenic Fuel Tank",
        "relationshipType": "composition",
        "description": "Valves are physically integrated within the tank structure for direct fuel line access."
      },
      {
        "source": "Cryogenic Filter",
        "target": "Variable Displacement Pump",
        "relationshipType": "dependency",
        "description": "The Filter must be installed upstream of the Pump to prevent contamination."
      }
    ],
