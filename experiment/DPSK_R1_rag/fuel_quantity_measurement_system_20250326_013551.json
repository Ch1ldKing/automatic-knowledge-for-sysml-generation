{
  "system_id": "fuel_quantity_measurement_system",
  "system_name": "Fuel Quantity Measurement System",
  "specification": {
    "designScenario": {
      "name": "Fuel Quantity Measurement System",
      "description": "The Fuel Quantity Measurement System (FQMS) is responsible for accurately determining and monitoring the aircraft's fuel quantity in real-time across all flight phases. It utilizes capacitive fuel probes, compensates for environmental variables like temperature and aircraft attitude, and integrates with flight management systems to provide critical data for fuel efficiency calculations, balance management, and safety compliance. The system incorporates redundancy, self-diagnostic capabilities, and fail-safe mechanisms to ensure reliability under diverse operational conditions.",
      "requirements": [
        {
          "title": "Fuel Level Sensing and Compensation",
          "type": "functional",
          "details": [
            {
              "description": "Measure fuel mass/volume in all tanks using capacitive probes with temperature compensation to account for fuel density variations."
            },
            {
              "description": "Implement aircraft attitude correction algorithms to compensate for fuel slosh and tank geometry during maneuvers."
            }
          ]
        },
        {
          "title": "Measurement Accuracy and Responsiveness",
          "type": "performance",
          "details": [
            {
              "description": "Maintain measurement accuracy within ±1% of total fuel capacity across operational temperature ranges."
            },
            {
              "description": "Update fuel quantity calculations at minimum 500ms intervals during critical flight phases."
            }
          ]
        },
        {
          "title": "Fault Tolerance and Integrity",
          "type": "safety",
          "details": [
            {
              "description": "Implement triple modular redundancy for probe signal processing with voting logic to isolate faulty channels."
            },
            {
              "description": "Provide continuous built-in test (BIT) capabilities to detect probe failures or signal degradation."
            }
          ]
        },
        {
          "title": "Maintenance and Certification",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Design modular probe interfaces compliant with ARINC 706 standards for simplified replacement."
            },
            {
              "description": "Meet DO-160G environmental qualifications for fuel tank EMI and lightning strike susceptibility."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Fuel Quantity Probes",
        "description": "Capacitive sensing units installed in fuel tanks measuring dielectric constant variations between fuel and air.",
        "type": "Sensing Assembly",
        "subComponents": [
          {
            "name": "Capacitive Sensor Array",
            "description": "Multiple concentric tube capacitors measuring fuel level through impedance changes.",
            "type": "Primary Sensor"
          },
          {
            "name": "Temperature Compensator",
            "description": "Integrated PT100 RTD for real-time dielectric constant compensation.",
            "type": "Compensation Module"
          }
        ]
      },
      {
        "name": "Measurement Processing Unit (MPU)",
        "description": "Central computer processing probe signals, implementing compensation algorithms, and managing redundancy.",
        "type": "Avionics Computer",
        "subComponents": [
          {
            "name": "Signal Conditioning Module",
            "description": "Converts analog probe signals to digital with 24-bit resolution.",
            "type": "Data Acquisition"
          },
          {
            "name": "Fault Detection and Isolation",
            "description": "Triple-channel comparator with mid-value selection logic.",
            "type": "Safety Processor"
          }
        ]
      },
      {
        "name": "Fuel Quantity Indicator",
        "description": "Cockpit display interface showing total/individual tank quantities and system status.",
        "type": "Crew Interface",
        "subComponents": [
          {
            "name": "Primary Display Module",
            "description": "LED/LCD display driver with ARINC 429 output",
            "type": "Display Controller"
          },
          {
            "name": "Low Fuel Alert Generator",
            "description": "Independent threshold comparator for crew alerts",
            "type": "Alert System"
          }
        ]
      },
      {
        "name": "Fuel Data Interface",
        "description": "Communication gateway sharing fuel data with FMS, ECAM, and maintenance systems.",
        "type": "Avionics Gateway",
        "subComponents": [
          {
            "name": "ARINC 429 Transceiver",
            "description": "Handles bidirectional communication with flight deck systems",
            "type": "Communication Module"
          },
          {
            "name": "Data Concentrator",
            "description": "Aggregates fuel data from multiple tanks into standardized formats",
            "type": "Data Processor"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Measurement Processing Unit (MPU)",
        "target": "Fuel Quantity Probes",
