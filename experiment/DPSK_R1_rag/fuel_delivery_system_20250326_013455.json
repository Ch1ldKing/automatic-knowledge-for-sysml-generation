      },
      {
        "source": "Cryogenic Cooling Jacket",
        "target": "Composite Fuel Cell",
        "relationshipType": "composition",
        "description": "Integrated cooling channels form structural part of fuel cell end caps"
      },
      {
        "source": "Phase Separation Detector",
        "target": "Composite Fuel Cell",
        "relationshipType": "aggregation",
        "description": "Optical probe array samples fuel from multiple cell quadrants for contamination analysis"
      },
      {
        "source": "Fuel Control Unit",
        "target": "Phase Separation Detector",
        "relationshipType": "dependency",
        "description": "FCU requires real-time water content data to adjust combustion parameters"
      }
    ],
