{
  "system_id": "ventilation_system",
  "system_name": "Ventilation System",
  "specification": {
    "designScenario": {
      "name": "Air Handling Unit",
      "description": "The Air Handling Unit (AHU) is a mission-critical subsystem within the aircraft's Environmental Control System (ECS), engineered to manage air supply, thermal regulation, and cabin pressurization. It integrates adaptive airflow algorithms, multi-stage filtration, and fail-safe redundancy to ensure passenger comfort and compliance with aviation safety standards. The AHU dynamically balances airflow distribution across flight phases, supports rapid decompression recovery, and interfaces with avionics for real-time health monitoring.",
      "requirements": [
        {
          "title": "Adaptive Airflow Management",
          "type": "functional",
          "details": [
            {
              "description": "The AHU shall automatically adjust airflow between 300-2500 CFM based on cabin occupancy sensors and flight altitude, maintaining a minimum air change rate of 15 cycles/hour."
            }
          ]
        },
        {
          "title": "Extreme Environment Operation",
          "type": "performance",
          "details": [
            {
              "description": "The system must maintain rated performance across -20°C to 55°C ambient temperatures and 0.6-1.5 atm pressure conditions during all flight operations."
            }
          ]
        },
        {
          "title": "Contaminant Neutralization",
          "type": "safety",
          "details": [
            {
              "description": "The AHU shall incorporate HEPA-grade filtration with virucidal coating, achieving >99.97% particulate removal efficiency for 0.3μm particles and neutralization of airborne pathogens."
            }
          ]
        },
        {
          "title": "Energy-Efficient Operation",
          "type": "nonFunctional",
          "details": [
            {
              "description": "The design must achieve ≥85% energy recovery through counter-flow heat exchangers and variable frequency fan drives, minimizing bleed air consumption."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Primary Air Module",
        "type": "Core Subsystem",
        "description": "Central unit housing airflow generation and conditioning elements, designed for 30,000-hour MTBF with hot-swappable components.",
        "subComponents": [
          {
            "name": "Dual-Stage Centrifugal Fan",
            "type": "Electromechanical Actuator",
            "description": "Twin counter-rotating impellers with magnetic bearing drives providing 200:1 turndown ratio and surge-free operation."
          },
          {
            "name": "Phase Change Heat Exchanger",
            "type": "Thermal Management",
            "description": "Microchannel design with paraffin-based thermal energy storage for load leveling during high-demand scenarios."
          }
        ]
      },
      {
        "name": "Air Quality Assembly",
        "type": "Safety Subsystem",
        "description": "Integrated filtration and gas-phase purification stack meeting DO-160G for smoke/toxin removal.",
        "subComponents": [
          {
            "name": "HEPA/UV-C Stack",
            "type": "Filtration",
            "description": "Multi-layer composite filter with 254nm ultraviolet germicidal irradiation chamber for microbial deactivation."
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Primary Air Module",
        "target": "Dual-Stage Centrifugal Fan",
        "relationshipType": "composition",
        "description": "The centrifugal fan is an integral part of the primary air module, with magnetic bearings directly mounted to the module's structural frame."
      },
      {
        "source": "Air Quality Assembly",
        "target": "HEPA/UV-C Stack",
        "relationshipType": "aggregation",
        "description": "The filtration stack is a replaceable subassembly within the air quality module, allowing cartridge replacement without system depressurization."
      },
      {
        "source": "Cabin Pressure Control",
        "target": "Air Handling Unit",
        "relationshipType": "dependency",
        "description": "The pressure control system requires real-time airflow data from the AHU to maintain cabin altitude profiles during ascent/descent."
      }
    ],
    "attributes": [
      {
        "name": "dynamicFlowRange",
        "type": "string",
        "value": "300 - 2500 CFM",
        "description": "Operational airflow capacity with automatic adjustment based on cabin pressure differentials and occupancy levels."
      },
      {
        "name": "thermalRecoveryEfficiency",
        "type": "string",
        "value": "≥85%",
        "description": "Minimum energy recovery performance across all operational modes via regenerative heat exchange."
      },
      {
        "name": "filterMERV",
