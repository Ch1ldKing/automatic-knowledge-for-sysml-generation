  "system_name": "Pump System",
  "specification": {
    "designScenario": {
      "name": "Hydraulic Pump",
      "description": "The Hydraulic Pump is a mission-critical component in modern aircraft, delivering precise hydraulic power for flight control surfaces, landing gear deployment, and emergency systems. It integrates adaptive pressure modulation, thermal resilience for extreme operational environments (-65째C to 200째C), and predictive health monitoring. The design prioritizes redundancy through dual-channel operation, electromagnetic interference (EMI) hardening for avionics compatibility, and lightweight composite materials to reduce overall system mass.",
      "requirements": [
        {
          "title": "Dual-Channel Pressure Output",
          "type": "functional",
          "details": [
            {
              "description": "The pump shall maintain independent primary and secondary pressure channels with automatic failover to ensure uninterrupted hydraulic supply during component degradation."
            }
          ]
        },
        {
          "title": "Dynamic Flow Regulation",
          "type": "performance",
          "details": [
            {
              "description": "The system shall adjust flow rates from 5 GPM to 85 GPM within 0.3 seconds to accommodate rapid changes in actuator demand during high-G maneuvers."
            }
          ]
        },
        {
          "title": "Thermal Runaway Prevention",
          "type": "safety",
          "details": [
            {
              "description": "Embedded phase-change cooling modules shall limit pump housing temperatures to 150째C maximum during continuous operation, with automatic emergency shutdown at 170째C."
            }
          ]
        },
        {
          "title": "Condition-Based Maintenance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "The pump shall incorporate vibration spectroscopy sensors and fluid particulate analysis to enable maintenance intervals based on actual wear instead of fixed flight hours."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Hydraulic Pump Core",
        "description": "Primary pressure generation unit with dual rotary piston assemblies and integrated smart valving",
        "type": "Electro-Hydraulic Actuator",
        "subComponents": [
          {
            "name": "Dual-Spool Drive Shaft",
            "description": "Titanium alloy shaft with harmonic dampeners transmitting mechanical power from multiple drive sources",
            "type": "Mechanical Transmission"
          },
          {
            "name": "Adaptive Plenum Chamber",
            "description": "Pressure-regulated cavity with shape-memory alloy walls optimizing fluid dynamics across operational envelopes",
            "type": "Fluidic Control"
          }
        ]
      },
      {
        "name": "Smart Control Module",
        "description": "FPGA-based controller implementing SAE AS5956 protocols for deterministic response in fly-by-wire systems",
        "type": "Avionics Interface Unit",
        "subComponents": [
          {
            "name": "Health Monitoring Processor",
            "description": "AI-accelerated chipset analyzing real-time wear patterns using convolutional neural networks",
            "type": "Diagnostic Subsystem"
          },
          {
            "name": "EMI-Shielded CAN Bus",
            "description": "Redundant controller area network interfaces meeting DO-254 Level A compliance",
            "type": "Communication Interface"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Hydraulic Pump Core",
        "target": "Smart Control Module",
        "relationshipType": "composition",
        "description": "The Smart Control Module is physically encapsulated within the Hydraulic Pump Core's EMI-hardened housing for signal integrity"
      },
      {
        "source": "Hydraulic Pump",
        "target": "Aircraft Power Distribution Unit",
        "relationshipType": "dependency",
        "description": "Requires 28VDC emergency power and 115VAC 400Hz primary power from the aircraft's electrical system"
      },
