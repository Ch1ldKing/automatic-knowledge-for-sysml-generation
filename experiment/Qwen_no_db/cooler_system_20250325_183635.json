        "subComponents": null
      },
      {
        "name": "Refrigerant Management System",
        "type": "Subsystem",
        "description": "Closed-loop refrigerant circuit with leak detection and recovery mechanisms.",
        "subComponents": [
          {
            "name": "Pressure Regulator",
            "description": "Automated valve maintaining optimal refrigerant pressure across flight altitudes.",
            "type": "Control Element"
          },
          {
