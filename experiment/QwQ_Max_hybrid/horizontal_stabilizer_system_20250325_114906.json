{
  "system_id": "horizontal_stabilizer_system",
  "system_name": "Horizontal Stabilizer System",
  "specification": {
    "designScenario": {
      "name": "Horizontal Stabilizer System",
      "description": "The Horizontal Stabilizer System is designed to maintain pitch stability and control the aircraft's longitudinal axis during flight. It provides precise pitch control through elevator deflection and trim adjustments, ensuring stability under varying flight conditions, load distributions, and external disturbances. The system integrates structural integrity, aerodynamic efficiency, and redundant actuation mechanisms to ensure safe operation across the aircraft's flight envelope.",
      "requirements": [
        {
          "title": "Pitch Stability Control",
          "type": "functional",
          "details": [
            {
              "description": "Maintain aircraft pitch equilibrium by adjusting elevator deflection in response to flight control inputs and aerodynamic forces."
            },
            {
              "description": "Enable automatic trim adjustments to reduce pilot workload during steady-state flight conditions."
            }
          ]
        },
        {
          "title": "Rapid Deflection Response",
          "type": "performance",
          "details": [
            {
              "description": "Achieve elevator deflection within 100 milliseconds of receiving actuation commands from the flight control system."
            },
            {
              "description": "Ensure smooth and continuous deflection adjustments during dynamic maneuvers."
            }
          ]
        },
        {
          "title": "Redundant Actuation and Failure Mitigation",
          "type": "safety",
          "details": [
            {
              "description": "Implement dual redundant actuators with independent power sources to prevent single-point failures."
            },
            {
              "description": "Trigger automatic pitch trim override and alert systems in case of actuator or sensor malfunction."
            }
          ]
        },
        {
          "title": "Structural and Environmental Robustness",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Withstand aerodynamic loads up to 3.5g positive and -1.5g negative load factors during extreme maneuvers."
            },
            {
              "description": "Resist environmental degradation via corrosion-resistant coatings and composite materials rated for -55°C to +90°C operational range."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Horizontal Stabilizer Assembly",
        "type": "Structural Assembly",
        "description": "The primary structural component integrating the stabilizer structure, elevators, and actuation systems to control pitch.",
        "subComponents": [
          {
            "name": "Stabilizer Structure",
            "type": "Aerodynamic Structure",
            "description": "The fixed airfoil section providing longitudinal stability and mounting base for movable elevator surfaces."
          },
          {
            "name": "Elevator Control Surfaces",
            "type": "Control Surface",
            "description": "Movable surfaces attached to the stabilizer structure, deflecting upward or downward to adjust aircraft pitch."
          },
          {
            "name": "Trim Tab",
            "type": "Trimming Mechanism",
            "description": "Small adjustable surface on the elevator to fine-tune pitch trim and reduce pilot control forces."
          },
          {
            "name": "Actuator System",
            "type": "Mechanical Actuator",
            "description": "Hydraulic/pneumatic/electric actuators providing the force to deflect elevator surfaces and trim tabs."
          }
        ]
      }
    ],
