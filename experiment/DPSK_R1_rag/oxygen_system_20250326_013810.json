      },
      {
        "source": "Oxygen Monitoring System",
        "target": "Oxygen Control Unit",
        "relationshipType": "association",
        "description": "The Monitoring System transmits real-time sensor data to the Control Unit via CAN bus."
      },
      {
        "source": "Distribution Network",
        "target": "Masks Deployment System",
        "relationshipType": "association",
        "description": "Oxygen ducts interface with mask lanyards to supply oxygen when masks are deployed."
      },
      {
        "source": "Oxygen Control Unit",
        "target": "Environmental Control System",
        "relationshipType": "dependency",
        "description": "The Oxygen System relies on cabin altitude data from the Environmental Control System's pressure sensors."
      }
    ],
