{
  "system_id": "landing_gear_braking_system",
  "system_name": "Landing Gear Braking System",
  "specification": {
    "designScenario": {
      "name": "Landing Gear Braking System",
      "description": "The Landing Gear Braking System is a critical subsystem within an aircraft's landing gear, designed to provide controlled deceleration during touchdown and taxiing. It integrates advanced braking mechanisms, heat management systems, and fail-safe redundancies to ensure safe and efficient aircraft stopping under varying operational conditions. The system must operate reliably under high kinetic energy dissipation, thermal stress, and potential hydraulic failures while complying with aviation safety and performance standards.",
      "requirements": [
        {
          "title": "Functional Braking Performance",
          "type": "functional",
          "details": [
            {
              "description": "The system must provide uniform braking force distribution across all landing gear wheels to prevent skidding or loss of directional control."
            },
            {
              "description": "Braking force must be adjustable based on aircraft weight, runway surface conditions, and pilot input."
            }
          ]
        },
        {
          "title": "Performance Efficiency",
          "type": "performance",
          "details": [
            {
              "description": "Achieve maximum deceleration of 4.5g during emergency stops without exceeding thermal limits of braking components."
            },
            {
              "description": "Maintain optimal braking efficiency after repeated high-energy landings through efficient heat dissipation."
            }
          ]
        },
        {
          "title": "Safety and Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "Implement dual hydraulic circuits to ensure continued braking capability in the event of a single circuit failure."
            },
            {
              "description": "Incorporate anti-lock braking (ABS) functionality to prevent wheel lockup during high-deceleration events."
            }
          ]
        },
        {
          "title": "Maintainability and Compliance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Design for easy access to brake components for routine inspections, pad replacement, and hydraulic fluid maintenance."
            },
            {
              "description": "Comply with FAA Part 25 standards for braking system reliability and environmental resistance (e.g., saltwater, dust, and temperature extremes)."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Brake System",
        "type": "Subsystem",
        "description": "Primary subsystem responsible for converting kinetic energy into thermal energy through friction, providing controlled deceleration.",
        "subComponents": [
          {
            "name": "Brake Calipers",
            "type": "Actuator",
            "description": "Hydraulically actuated components that clamp brake discs to generate friction force proportional to applied hydraulic pressure."
          },
          {
            "name": "Brake Discs",
            "type": "Friction Component",
            "description": "Rotating discs attached to the wheel hubs that provide friction surfaces for deceleration when engaged by brake calipers."
          },
          {
            "name": "Anti-lock Braking Module (ABS)",
            "type": "Control System",
            "description": "Electro-hydraulic control unit that modulates brake pressure to prevent wheel lockup during high-deceleration events."
          }
        ]
      },
      {
        "name": "Hydraulic System",
        "type": "Subsystem",
        "description": "Provides pressurized hydraulic fluid to actuate brake calipers and control braking force magnitude.",
        "subComponents": null
      },
      {
        "name": "Wheel Assembly",
        "type": "Mechanical Assembly",
        "description": "Integral component housing wheels, brake discs, and attachment points to the landing gear strut.",
        "subComponents": null
      },
      {
        "name": "Brake Temperature Monitoring System",
        "type": "Sensor System",
        "description": "Incorporates thermocouples and control interfaces to monitor disc and caliper temperatures, alerting pilots to overheating conditions.",
        "subComponents": null
      }
    ],
    "relationships": [
      {
        "source": "Brake System",
        "target": "Brake Calipers",
        "relationshipType": "composition",
        "description": "The Brake System physically integrates brake calipers as a critical subcomponent for force application."
      },
      {
        "source": "Brake System",
        "target": "Brake Discs",
        "relationshipType": "composition",
        "description": "Brake discs are an integral part of the Brake System, mounted to the wheel assembly to enable friction-based deceleration."
      },
      {
        "source": "Brake System",
        "target": "Hydraulic System",
        "relationshipType": "dependency",
        "description": "The Brake System relies on hydraulic pressure from the Hydraulic System to actuate calipers and modulate braking force."
      },
      {
        "source": "Brake System",
        "target": "Wheel Assembly",
        "relationshipType": "association",
        "description": "The Brake System interfaces with the Wheel Assembly to transfer braking torque from calipers to the aircraft's landing gear."
      },
      {
        "source": "Brake System",
        "target": "Brake Temperature Monitoring System",
        "relationshipType": "dependency",
        "description": "The Brake System depends on temperature monitoring data to ensure safe thermal operation limits are maintained."
      }
    ],
    "attributes": [
      {
        "name": "maxDeceleration",
        "type": "string",
        "value": "4.5g",
        "description": "Maximum deceleration achievable under emergency braking conditions without compromising structural integrity."
      },
      {
        "name": "heatDissipation",
        "type": "string",
        "value": "High capacity with directional airflow channels",
        "description": "Thermal management design using aerodynamic cooling channels to dissipate heat during prolonged braking."
      },
      {
        "name": "responseTime",
        "type": "string",
        "value": "≤0.3 seconds",
        "description": "Time from pilot input to full hydraulic actuation of brake calipers under nominal conditions."
      },
      {
        "name": "redundancy",
        "type": "string",
        "value": "Dual hydraulic circuits with independent control valves",
        "description": "Two independent hydraulic circuits ensuring continued braking capability even if one circuit fails."
      },
      {
        "name": "brakePadWearLimit",
        "type": "string",
        "value": "3mm",
        "description": "Maximum allowable thickness of brake pad material before replacement is required."
      },
      {
        "name": "maxOperatingTemp",
        "type": "string",
        "value": "600°C",
        "description": "Maximum safe operating temperature for brake discs and calipers under continuous braking."
      }
    ]
  },
