{
  "system_id": "distribution_system",
  "system_name": "Distribution System",
  "specification": {
    "designScenario": {
      "name": "Fluid Distribution System",
      "description": "The Fluid Distribution System (FDS) is a mission-critical subsystem responsible for adaptive thermal management and precision delivery of de-icing/anti-icing fluids across all protected surfaces. It integrates smart flow modulation, real-time viscosity compensation, and predictive ice detection algorithms to optimize fluid usage. The system features a triple-redundant architecture with self-diagnostic capabilities, ensuring compliance with EASA CS-25 and FAA AC 20-73C standards while maintaining compatibility with next-generation sustainable aviation fluids.",
      "requirements": [
        {
          "title": "Adaptive Flow Modulation",
          "type": "functional",
          "details": [
            {
              "description": "The system shall autonomously adjust flow characteristics based on real-time sensor inputs including airspeed, outside air temperature, and ice detection radar signals using PID-controlled proportional valves."
            }
          ]
        },
        {
          "title": "Thermal Performance Specifications",
          "type": "performance",
          "details": [
            {
              "description": "Must maintain fluid viscosity between 15-300 cP across operational temperatures (-40°C to +70°C) while sustaining flow rates of 75-150 L/min at 1.5-6 bar dynamic pressure."
            }
          ]
        },
        {
          "title": "Fault-Tolerant Architecture",
          "type": "safety",
          "details": [
            {
              "description": "Implement triple modular redundancy (TMR) in pump control circuits with cross-channel monitoring and automatic isolation of degraded channels per DO-254 Level A requirements."
            }
          ]
        },
        {
          "title": "Lifecycle Maintainability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Field-replaceable units (FRUs) shall be swappable within 15 minutes MTTR using standard line maintenance tools, with health monitoring via ARINC 664 interfaces."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Smart Distribution Hub",
        "type": "Control Unit",
        "description": "Central processing unit with dual FPGAs for real-time fluid dynamics modeling and decision-making, incorporating MEMS-based pressure/temperature sensor fusion",
        "subComponents": [
          {
            "name": "Fluid Dynamics Processor",
            "description": "Dual-channel computing module executing computational fluid dynamics (CFD) algorithms for flow optimization",
            "type": "Avionics Module"
          },
          {
            "name": "Health Monitoring Unit",
            "description": "Continuous condition-based monitoring subsystem with prognostic capabilities for pump wear prediction",
            "type": "Diagnostic Module"
          }
        ]
      },
      {
        "name": "High-Efficiency Pump Assembly",
        "type": "Actuation System",
        "description": "Triple-redundant positive displacement pumps with titanium alloy internals rated for 20,000 PSI cyclic operation",
        "subComponents": [
          {
            "name": "Main Drive Pump",
            "description": "Primary flow generator with variable frequency drive (VFD) motor for precision RPM control",
            "type": "Rotodynamic Pump"
          },
          {
            "name": "Backup Cartridge Pump",
            "description": "Sealed standby unit with pyro-activated quick-connect for emergency deployment",
            "type": "Positive Displacement Pump"
          }
        ]
      },
      {
        "name": "Thermal Regulation Unit",
        "type": "Conditioning System",
        "description": "Active temperature management subsystem maintaining fluid viscosity through Peltier-effect heating/cooling plates",
        "subComponents": [
          {
            "name": "Viscosity Compensator",
            "description": "Multi-zone heat exchanger with graphene-enhanced thermal transfer surfaces",
            "type": "Thermal Module"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Smart Distribution Hub",
        "target": "High-Efficiency Pump Assembly",
        "relationshipType": "composition",
        "description": "The hub directly controls and monitors all pump assembly operations through MIL-STD-1553B data buses"
      },
      {
        "source": "Thermal Regulation Unit",
        "target": "High-Efficiency Pump Assembly",
        "relationshipType": "aggregation",
        "description": "Thermal management system shares physical interfaces with pump housing but operates as independent subsystem"
      },
      {
        "source": "Fluid Distribution System",
        "target": "Aircraft Central Computer",
        "relationshipType": "dependency",
        "description": "Receives flight phase data and environmental parameters via AFDX network for system mode selection"
      }
    ],
    "attributes": [
      {
