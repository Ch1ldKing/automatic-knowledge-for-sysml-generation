{
  "system_id": "oxygen_system",
  "system_name": "Oxygen System",
  "specification": {
    "designScenario": {
      "name": "Oxygen System",
      "description": "The Oxygen System is a critical safety system designed to provide breathable oxygen to passengers and crew during cabin decompression events or loss of cabin pressure. It integrates chemical oxygen generators, distribution networks, and mask deployment mechanisms to ensure rapid oxygen supply. The system is engineered for high reliability, fail-safe operation, and compliance with aviation safety regulations (e.g., FAA, EASA). It operates in tandem with the Environmental Control System to monitor cabin altitude and trigger emergency oxygen deployment when thresholds are exceeded.",
      "requirements": [
        {
          "title": "Oxygen Generation",
          "type": "functional",
          "details": [
            {
              "description": "The system shall generate breathable oxygen via chemical oxygen generators or stored gaseous oxygen, activated automatically during cabin depressurization."
            }
          ]
        },
        {
          "title": "Mask Deployment",
          "type": "functional",
          "details": [
            {
              "description": "Automatically deploy passenger and crew oxygen masks within 5 seconds of cabin altitude exceeding 14,000 feet."
            }
          ]
        },
        {
          "title": "Oxygen Flow Rate",
          "type": "performance",
          "details": [
            {
              "description": "Deliver a minimum oxygen flow rate of 2.4 liters per minute per passenger mask for at least 15 minutes."
            }
          ]
        },
        {
          "title": "Redundant Activation",
          "type": "safety",
          "details": [
            {
              "description": "Implement dual redundant activation pathways (electrical and mechanical) to ensure mask deployment under all failure conditions."
            }
          ]
        },
        {
          "title": "Monitoring Interface",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Provide real-time oxygen system status and fault diagnostics to the cockpit crew via the central Environmental Control System interface."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Oxygen Generators",
        "description": "Chemical or gaseous oxygen sources that produce breathable oxygen upon activation.",
        "type": "Subsystem",
        "subComponents": [
          {
            "name": "Chemical Oxygen Canisters",
            "description": "Sodium chlorate-based generators activated by pyrotechnic charges during decompression.",
            "type": "Oxygen Source"
          },
          {
            "name": "Activation Mechanism",
            "description": "Electromechanical trigger initiating oxygen generation via thermal decomposition.",
            "type": "Actuator"
          },
          {
            "name": "Oxygen Purity Sensor",
            "description": "Monitors O2 concentration in generated gas to ensure ≥99% purity.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Distribution Network",
        "description": "Ducting and valves regulating oxygen flow from generators to masks.",
        "type": "Subsystem",
        "subComponents": [
          {
            "name": "Oxygen Ducts",
            "description": "Fire-resistant polymer conduits distributing oxygen to mask assemblies.",
            "type": "Conduit"
          },
          {
            "name": "Flow Control Valves",
            "description": "Electropneumatic valves modulating oxygen pressure and flow rates.",
            "type": "Actuator"
          },
          {
            "name": "Pressure Regulator",
            "description": "Maintains 4-6 psi outlet pressure for mask delivery systems.",
            "type": "Regulator"
          }
        ]
      },
      {
        "name": "Masks Deployment System",
        "description": "Mechanism storing and releasing passenger/crew oxygen masks.",
        "type": "Subsystem",
        "subComponents": [
          {
            "name": "Mask Assemblies",
            "description": "Oxygen masks with rebreather bags and elastic lanyards.",
            "type": "Safety Device"
          },
          {
            "name": "Release Mechanism",
            "description": "Spring-loaded panel doors activated by cabin altitude sensors.",
            "type": "Actuator"
          },
          {
            "name": "Lanyard Sensor",
            "description": "Detects mask pull-to-activate action to initiate oxygen flow.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Oxygen Monitoring System",
        "description": "Sensors and controllers tracking oxygen system status.",
        "type": "Monitoring Module",
        "subComponents": [
          {
            "name": "Cabin Altitude Sensor",
            "description": "Barometric sensor triggering deployment at 14,000 ft cabin altitude.",
            "type": "Sensor"
          },
          {
            "name": "Oxygen Pressure Sensor",
            "description": "Measures line pressure in distribution network.",
            "type": "Sensor"
          }
        ]
      },
      {
        "name": "Oxygen Control Unit",
        "description": "Central processor managing activation thresholds and crew alerts.",
        "type": "Electronic Module",
        "subComponents": [
          {
            "name": "Fault Detection Circuit",
            "description": "Continuous built-in-test (BIT) of oxygen system components.",
            "type": "Diagnostic Module"
          },
          {
            "name": "Crew Alerting Panel",
            "description": "Visual/aural warnings for low oxygen pressure or generator faults.",
            "type": "Interface"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Oxygen Control Unit",
        "target": "Oxygen Monitoring System",
        "relationshipType": "aggregation",
        "description": "The Control Unit aggregates pressure and altitude data from the Monitoring System to determine deployment timing."
      },
      {
