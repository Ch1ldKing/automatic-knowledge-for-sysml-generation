{
  "system_id": "intercom_system",
  "system_name": "Intercom System",
  "specification": {
    "designScenario": {
      "name": "Cabin Communication System",
      "description": "An advanced digital communication platform integrating voice/data transmission, crew coordination, and passenger services. The system employs redundant fiber-optic/Ethernet networks with MIL-STD-810G environmental tolerance, supporting simultaneous operation of crew intercom, automated passenger announcements, and emergency coordination through a real-time operating system (RTOS). Interfaces with Flight Deck Communication System and Cabin Management System for synchronized operations.",
      "requirements": [
        {
          "title": "Core Communication Services",
          "type": "functional",
          "details": [
            {
              "description": "Multi-channel digital intercom supporting simultaneous crew-station communications with priority override"
            },
            {
              "description": "Automated multilingual public address system with text-to-speech conversion"
            },
            {
              "description": "Integrated emergency coordination channel with flight deck override capability"
            }
          ]
        },
        {
          "title": "Operational Performance",
          "type": "performance",
          "details": [
            {
              "description": "End-to-end audio latency <500ms across all communication channels"
            },
            {
              "description": "99.999% system availability through redundant network architecture"
            },
            {
              "description": "Concurrent support for 64+ voice/data channels with dynamic bandwidth allocation"
            }
          ]
        },
        {
          "title": "Certification Compliance",
          "type": "safety",
          "details": [
            {
              "description": "Independent backup communication path meeting FAA/EASA CS-25.1431(c) requirements"
            },
            {
              "description": "Galvanic isolation between critical subsystems to prevent fault propagation"
            },
            {
              "description": "Smoke/toxicity compliance per ABD0031-100 for all installed components"
            }
          ]
        },
        {
          "title": "System Integrity",
          "type": "nonFunctional",
          "details": [
            {
              "description": "AES-256 encrypted voice/data channels with FIPS 140-2 validated cryptographic modules"
            },
            {
              "description": "Modular LRU design supporting <15 minute hot-swap replacement per ATA iSpec 2200"
            },
            {
              "description": "Self-diagnostic system with prognostic health monitoring (PHM) capabilities"
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Central Communication Controller",
        "type": "Avionics Computer",
        "description": "Dual-channel fault-tolerant computer managing communication routing, prioritization, and system health monitoring",
        "subComponents": [
          {
            "name": "Packet Switching Matrix",
            "type": "Network Processor",
            "description": "Layer 3 managed switch providing deterministic latency QoS for voice/video/data traffic"
          },
          {
            "name": "Crypto Management Unit",
            "type": "Security Module",
            "description": "Hardware security module (HSM) for key management and secure protocol processing"
          }
        ]
      },
      {
        "name": "Crew Terminal Network",
        "type": "Human-Machine Interface",
        "description": "Distributed touchscreen interfaces with noise-adaptive microphones and bone conduction headsets",
        "subComponents": [
          {
            "name": "Voice Processing Unit",
            "type": "DSP Module",
            "description": "Beamforming array processor with active noise cancellation (ANC) algorithms"
          },
          {
            "name": "Emergency Call Panel",
            "type": "Control Interface",
            "description": "Dedicated hardwired communication panel with tactile controls"
          }
        ]
      },
      {
        "name": "Passenger Address System",
        "type": "Audio Distribution Network",
        "description": "Multi-zone Class-D amplification system with impedance monitoring and automatic load balancing",
        "subComponents": [
          {
            "name": "Digital Announcement Manager",
            "type": "Content Router",
            "description": "Synchronizes pre-recorded messages across cabin zones with ambient noise compensation"
          },
          {
            "name": "PA Amplifier Rack",
            "type": "Power Electronics",
            "description": "100V line array amplifiers with short-circuit protection and thermal derating"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Central Communication Controller",
        "target": "Crew Terminal Network",
        "relationshipType": "composition",
        "description": "Controller manages configuration and security policies for all crew terminal operations"
      },
      {
        "source": "Cabin Communication System",
        "target": "Flight Deck ICS",
        "relationshipType": "association",
        "description": "Synchronized communication states through ARINC 758-compliant digital interface"
      },
      {
        "source": "Passenger Address System",
        "target": "Cabin Management System",
        "relationshipType": "aggregation",
        "description": "Shared audio infrastructure with separate control paths for passenger entertainment and safety announcements"
      }
    ],
