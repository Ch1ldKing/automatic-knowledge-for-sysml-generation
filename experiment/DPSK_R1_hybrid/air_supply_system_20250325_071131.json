{
  "system_id": "air_supply_system",
  "system_name": "Air Supply System",
  "specification": {
    "designScenario": {
      "name": "Air Supply System",
      "description": "The Air Supply System (ASS) is a critical subsystem of the Environmental Control System (ECS) responsible for sourcing, conditioning, and distributing breathable air to the aircraft cabin. It integrates bleed air extraction from engines/APU, filtration, temperature preconditioning, and pressure regulation to ensure a continuous supply of air meeting aviation safety standards. The system maintains cabin pressurization, oxygen levels, and air circulation while coordinating with fire suppression and emergency oxygen systems.",
      "requirements": [
        {
          "title": "Airflow Regulation",
          "type": "functional",
          "details": [
            {
              "description": "The system shall regulate airflow between 800-2500 CFM based on cabin occupancy and flight phase using variable-frequency fan drives."
            }
          ]
        },
        {
          "title": "Cabin Pressurization",
          "type": "functional",
          "details": [
            {
              "description": "Maintain cabin pressure altitude below 8,000 ft (0.75-1.1 atm) during all flight phases using outflow valve control."
            }
          ]
        },
        {
          "title": "Redundant Air Sources",
          "type": "safety",
          "details": [
            {
              "description": "Provide dual redundant air sources (engine bleed/APU/electric compressor) with automatic failover within 3 seconds of pressure loss."
            }
          ]
        },
        {
          "title": "Particulate Filtration",
          "type": "functional",
          "details": [
            {
              "description": "Remove 99.97% of airborne particulates ≥0.3 microns using HEPA filters with differential pressure monitoring."
            }
          ]
        },
        {
          "title": "Emergency Pressurization",
          "type": "performance",
          "details": [
            {
              "description": "Maintain cabin pressure during rapid decompression events with <15 second recovery to safe levels (≤15,000 ft equivalent)."
            }
          ]
        },
        {
          "title": "Crew Interface",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Provide touchscreen controls with synoptic displays showing airflow rates, filter status, and pressure differentials."
            }
          ]
        },
        {
          "title": "Line Replaceable Units",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Design all components as LRUs (Line Replaceable Units) with <30 minute MTTR (Mean Time To Repair)."
            }
          ]
        }
      ]
    },
