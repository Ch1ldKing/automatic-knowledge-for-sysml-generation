{
  "system_id": "main_landing_gear_system",
  "system_name": "Main Landing Gear System",
  "specification": {
    "designScenario": {
      "name": "Advanced Landing Gear System (ALGS)",
      "description": "Design an advanced landing gear system integrating smart sensors, lightweight composite materials, and triple redundancy for extreme operational conditions. The system must autonomously manage deployment/retraction, shock absorption, and braking while maintaining structural integrity under high loads. It should support autonomous emergency protocols, real-time health monitoring, and extended maintenance intervals.",
      "requirements": [
        {
          "title": "Autonomous Deployment and Retraction",
          "type": "functional",
          "details": [
            {
              "description": "The system must deploy and retract autonomously based on altitude, speed, and aircraft configuration signals."
            },
            {
              "description": "Manual override capability must coexist with autonomous operations."
            }
          ]
        },
        {
          "title": "Shock Absorption and Energy Dissipation",
          "type": "functional",
          "details": [
            {
              "description": "The system must absorb at least 95% of vertical impact energy during landing."
            },
            {
              "description": "Damping mechanisms must adapt to variable load conditions in real-time."
            }
          ]
        },
        {
          "title": "Performance Metrics",
          "type": "performance",
          "details": [
            {
              "description": "Full deployment/retraction must occur within 2.5 seconds under nominal conditions."
            },
            {
              "description": "Operate effectively at temperatures ranging from -54째C to 80째C."
            }
          ]
        },
        {
          "title": "Triple Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "All critical actuators and control systems must have three independent failure paths."
            },
            {
              "description": "Emergency deployment must be achievable via independent mechanical actuators if hydraulic/electrical systems fail."
            }
          ]
        },
        {
          "title": "Lightweight and Durability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Structural components must utilize carbon fiber composites to reduce weight by 50% compared to traditional systems."
            },
            {
              "description": "System must maintain structural integrity after 5,000 flight hours under standard operating conditions."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Smart Control Unit",
        "description": "AI-driven control system for autonomous operations, real-time diagnostics, and redundancy management.",
        "type": "Electronic Module",
        "subComponents": [
          {
            "name": "AI Processor Module",
            "description": "Handles predictive analytics and decision-making for autonomous operations.",
            "type": "Processor"
          },
          {
            "name": "Sensor Interface Board",
            "description": "Manages data flow from environmental and structural sensors.",
            "type": "Interface Module"
          }
        ]
      },
      {
        "name": "Advanced Hydraulic System",
        "description": "High-pressure hydraulic subsystem with adaptive flow control for actuation and braking.",
        "type": "Subsystem",
        "subComponents": [
          {
            "name": "Variable Displacement Pump",
            "description": "Adjusts output based on system demand to optimize energy consumption.",
            "type": "Actuator"
          },
          {
            "name": "Thermal Management Unit",
            "description": "Maintains hydraulic fluid temperature within operational limits.",
            "type": "Cooling System"
          }
        ]
      },
      {
        "name": "Lightweight Structural Assembly",
        "description": "Composite-based structural framework providing high strength-to-weight ratio.",
        "type": "Mechanical Assembly",
        "subComponents": [
          {
            "name": "Carbon Fiber Strut",
            "description": "Primary load-bearing component manufactured from aerospace-grade composites.",
            "type": "Structural Component"
          },
          {
            "name": "Adaptive Damping System",
            "description": "Fluid-filled dampers with real-time stiffness adjustment.",
            "type": "Damping Device"
          }
        ]
      },
      {
        "name": "Integrated Braking System",
        "description": "Hydro-mechanical braking with regenerative energy capture.",
        "type": "Subsystem",
        "subComponents": [
          {
            "name": "Regenerative Brake Caliper",
            "description": "Converts kinetic energy into electrical energy during braking.",
            "type": "Actuator"
          },
          {
            "name": "Carbon-Carbon Brake Disc",
            "description": "Heat-resistant disc for high-performance braking.",
            "type": "Friction Component"
          }
        ]
      },
      {
        "name": "Environmental Sensor Array",
        "description": "Multi-sensor package monitoring temperature, pressure, and structural health.",
        "type": "Monitoring Module",
        "subComponents": [
          {
            "name": "Strain Gauge Network",
            "description": "Measures stress distribution across structural components.",
            "type": "Sensor"
          },
          {
            "name": "Thermocouple Array",
            "description": "Monitors temperature gradients in critical zones.",
            "type": "Sensor"
          }
        ]
      }
    ],
