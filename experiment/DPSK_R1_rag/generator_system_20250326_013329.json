{
  "system_id": "generator_system",
  "system_name": "Generator System",
  "specification": {
    "designScenario": {
      "name": "Generator System",
      "description": "Design a high-reliability Generator System to produce and condition electrical power for aircraft subsystems, ensuring seamless integration with the Electrical Power System. The system must maintain stable power generation under variable engine speeds, extreme altitudes, and transient loads while adhering to aviation safety standards.",
      "requirements": [
        {
          "title": "Power Generation & Conditioning",
          "type": "functional",
          "details": [
            {
              "description": "Generate 3-phase AC power from engine-driven generators or auxiliary power units (APUs)."
            },
            {
              "description": "Provide automatic voltage regulation (AVR) to maintain output within ±1% of nominal voltage."
            }
          ]
        },
        {
          "title": "Operational Performance",
          "type": "performance",
          "details": [
            {
              "description": "Deliver 90 kVA continuous power output per generator channel at 115V AC, 400 Hz."
            },
            {
              "description": "Maintain <3% total harmonic distortion (THD) under full load conditions."
            }
          ]
        },
        {
          "title": "Fault Tolerance",
          "type": "safety",
          "details": [
            {
              "description": "Implement differential current protection to detect and isolate ground faults."
            },
            {
              "description": "Include overfrequency/underfrequency protection to prevent generator overspeed damage."
            }
          ]
        },
        {
          "title": "Environmental Compliance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Operate reliably in altitudes up to 50,000 ft with -55°C to +70°C ambient temperature."
            },
            {
              "description": "Meet 2000-hour mean time between failures (MTBF) for critical subassemblies."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Main Generator Unit",
        "description": "Engine-driven permanent magnet generator producing primary aircraft power, comparable to integrated drive generators (IDGs) in modern turbofans.",
        "type": "Power Generation Module",
        "subComponents": [
          {
            "name": "Rotor Assembly",
            "description": "High-speed rotating component with rare-earth magnets for electromagnetic induction.",
            "type": "Electromechanical Assembly"
          },
          {
            "name": "Stator Windings",
            "description": "Stationary copper coil assembly generating 3-phase AC output.",
            "type": "Electrical Component"
          }
        ]
      },
      {
        "name": "APU Generator",
        "description": "Auxiliary power unit generator providing ground/emergency power, analogous to aircraft APU bleed-air systems.",
        "type": "Backup Power Module",
        "subComponents": [
          {
            "name": "Starter-Generator",
            "description": "Dual-function unit enabling APU start-up and power generation modes.",
            "type": "Hybrid Component"
          },
          {
            "name": "Fuel Control Unit",
            "description": "Hydro-mechanical regulator maintaining APU RPM within operational limits.",
            "type": "Control Subsystem"
          }
        ]
      },
      {
        "name": "Voltage Regulation Unit",
        "description": "Solid-state automatic voltage regulator maintaining output stability despite engine speed variations.",
        "type": "Conditioning Module",
        "subComponents": [
          {
            "name": "Excitation Controller",
            "description": "Adjusts field current to maintain constant generator output voltage.",
            "type": "Feedback Controller"
          },
          {
            "name": "Harmonic Filter",
            "description": "Passive LC filter network reducing waveform distortion.",
            "type": "Electrical Filter"
          }
        ]
      },
      {
        "name": "Generator Control Unit (GCU)",
        "description": "Digital control module monitoring generator health and implementing protective functions.",
        "type": "Monitoring Module",
        "subComponents": [
          {
            "name": "Fault Detection Processor",
            "description": "Dual-channel FPGA analyzing current/voltage signatures for anomaly detection.",
            "type": "Digital Controller"
          },
          {
            "name": "Load Shedding Circuit",
            "description": "Prioritizes power distribution during under-voltage conditions.",
            "type": "Protection Subsystem"
          }
        ]
      },
      {
        "name": "Cooling System",
        "description": "Air-oil heat exchanger maintaining generator temperatures within limits, similar to aircraft bleed air cooling architectures.",
        "type": "Thermal Management Module",
        "subComponents": [
          {
            "name": "Oil Circulation Pump",
            "description": "Positive displacement pump providing forced lubrication/cooling.",
            "type": "Mechanical Subsystem"
          },
          {
            "name": "Heat Exchanger Core",
            "description": "Aluminum finned-tube radiator dissipating generator heat.",
            "type": "Thermal Component"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Main Generator Unit",
        "target": "Engine Drive Shaft",
        "relationshipType": "dependency",
        "description": "Mechanically coupled to the aircraft engine via constant speed drive (CSD) for torque conversion."
      },
      {
        "source": "APU Generator",
        "target": "Main Generator Unit",
        "relationshipType": "association",
        "description": "Synchronizes with primary generators during cross-bus power transfers."
      },
      {
        "source": "Generator Control Unit (GCU)",
        "target": "Voltage Regulation Unit",
        "relationshipType": "aggregation",
        "description": "Collects real-time voltage/current data from AVR sensors for closed-loop control."
      },
      {
        "source": "Cooling System",
        "target": "Main Generator Unit",
        "relationshipType": "composition",
        "description": "Integrated cooling jacket forms structural part of generator housing."
      },
      {
        "source": "Generator System",
        "target": "Electrical Power System",
        "relationshipType": "dependency",
        "description": "Feeds conditioned AC power to the central electrical bus for distribution."
      }
    ],
    "attributes": [
      {
