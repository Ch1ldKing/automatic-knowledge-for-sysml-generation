{
  "system_id": "actuator_system",
  "system_name": "Actuator System",
  "specification": {
    "designScenario": {
      "name": "Actuation System",
      "description": "The Actuation System is a critical subsystem responsible for converting control signals into precise mechanical movements for aircraft control surfaces, landing gear, and braking systems. It integrates hydraulic and electric actuation technologies, ensuring reliable operation under extreme environmental conditions while maintaining compatibility with advanced flight control systems.",
      "requirements": [
        {
          "title": "Actuation Functionality",
          "type": "functional",
          "details": [
            {
              "description": "The system must convert electrical or hydraulic control signals into linear, rotational, or combined motion to operate control surfaces, landing gear, and braking mechanisms with sub-millimeter precision."
            }
          ]
        },
        {
          "title": "High-Performance Response",
          "type": "performance",
          "details": [
            {
              "description": "The actuation response time must not exceed 0.3 seconds under nominal conditions to support rapid maneuver execution during flight phases requiring high agility."
            }
          ]
        },
        {
          "title": "Fail-Safe Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "Actuators must default to a predefined safe position (e.g., neutral for control surfaces, locked for landing gear) in the event of power loss, hydraulic failure, or signal interruption."
            }
          ]
        },
        {
          "title": "Environmental and Operational Robustness",
          "type": "nonFunctional",
          "details": [
            {
              "description": "The system must withstand vibration, electromagnetic interference (EMI), and temperature extremes while maintaining operational integrity for the aircraft's entire service life."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Actuation System",
        "description": "The primary module managing the conversion of control signals to mechanical motion across the aircraft.",
        "type": "Hybrid Actuation Module",
        "subComponents": [
          {
            "name": "Hydraulic Actuator",
            "description": "Provides high-force linear or rotational motion using pressurized hydraulic fluid for heavy-duty applications like landing gear deployment.",
            "type": "Piston Actuator"
          },
          {
            "name": "Electric Actuator",
            "description": "Delivers precise, energy-efficient motion for control surfaces using electric motors, suitable for low-force, high-precision applications.",
            "type": "Servo Actuator"
          },
          {
            "name": "Feedback Sensor Array",
            "description": "Monitors actuator position, force, and velocity in real time, providing closed-loop control to the flight management system.",
            "type": "Sensor Subsystem"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Flight Control Computer",
        "target": "Actuation System",
        "relationshipType": "association",
        "description": "The Flight Control Computer sends digital command signals to the Actuation System to initiate actuator movements based on pilot or autopilot inputs."
      },
      {
        "source": "Actuation System",
        "target": "Central Health Monitoring System",
        "relationshipType": "aggregation",
        "description": "The Actuation System provides telemetry data (e.g., position, temperature, fault codes) to the Central Health Monitoring System for real-time diagnostics."
      },
      {
        "source": "Actuation System",
        "target": "Hydraulic Actuator",
        "relationshipType": "composition",
        "description": "The Hydraulic Actuator is a core subcomponent of the Actuation System, responsible for high-force applications."
      },
      {
        "source": "Actuation System",
        "target": "Electric Actuator",
        "relationshipType": "composition",
        "description": "The Electric Actuator is integrated into the Actuation System for low-force, high-precision tasks."
      },
      {
        "source": "Electric Actuator",
        "target": "Electrical Power Supply",
        "relationshipType": "dependency",
        "description": "The Electric Actuator depends on the Electrical Power Supply for operational energy."
      }
    ],
    "attributes": [
      {
