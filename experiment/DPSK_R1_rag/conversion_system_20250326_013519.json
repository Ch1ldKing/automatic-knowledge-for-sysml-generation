{
  "system_id": "conversion_system",
  "system_name": "Conversion System",
  "specification": {
    "designScenario": {
      "name": "AC-DC Converter",
      "description": "A high-reliability AC-DC power converter designed for next-generation commercial and military aircraft. The system integrates adaptive power management, fault-tolerant architecture, and advanced thermal regulation to support 24/7 operation in high-altitude environments. It ensures seamless compatibility with hybrid-electric propulsion systems and next-gen avionics while adhering to AS9100D and DO-354 standards.",
      "requirements": [
        {
          "title": "Adaptive Power Conversion",
          "type": "functional",
          "details": [
            {
              "description": "Convert variable-frequency aircraft AC power (360-800Hz) to stabilized DC output with <2% voltage deviation during engine start-up transients."
            },
            {
              "description": "Implement automatic phase synchronization for parallel operation with redundant power buses."
            }
          ]
        },
        {
          "title": "Advanced Power Density",
          "type": "performance",
          "details": [
            {
              "description": "Achieve power density ≥1.2kW/kg with 96.5% peak efficiency at 75% load (MIL-STD-704F)"
            },
            {
              "description": "Maintain <100mVp-p output ripple under 10-100% load steps within 50μs"
            }
          ]
        },
        {
          "title": "Fault-Tolerant Operation",
          "type": "safety",
          "details": [
            {
              "description": "Implement arc fault circuit interruption (AFCI) with <5ms response time"
            },
            {
              "description": "Galvanic isolation meeting DAL-A requirements with 5kV dielectric withstand"
            }
          ]
        },
        {
          "title": "Condition-Based Maintenance",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Embedded prognostic health monitoring with MTBCF >50,000 flight hours"
            },
            {
              "description": "Conformal coating for operation in 100% relative humidity with fungus resistance (MIL-STD-810H)"
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Active Front-End Rectifier",
        "type": "Power Stage",
        "description": "Three-phase PWM rectifier with harmonic compensation (<3% THD) and unity power factor correction",
        "subComponents": [
          {
            "name": "SiC MOSFET Bridge",
            "description": "Silicon carbide-based switching matrix for 100kHz operation",
            "type": "Semiconductor Array"
          },
          {
            "name": "EMI Supression Filter",
            "description": "Multi-stage filter meeting DO-160G Section 21 Category Z",
            "type": "Filter Module"
          }
        ]
      },
      {
        "name": "Isolated DC-DC Stage",
        "type": "Conversion Core",
        "description": "LLC resonant converter with wide-input ZVS operation (200-800VDC input range)",
        "subComponents": [
          {
            "name": "Planar Transformer",
            "description": "Embedded magnetics with integrated cooling channels",
            "type": "Magnetic Component"
          },
          {
            "name": "Synchronous Rectification",
            "description": "Adaptive dead-time control for reverse current prevention",
            "type": "Control Circuit"
          }
        ]
      },
      {
        "name": "Intelligent Thermal Manager",
        "type": "Control Module",
        "description": "Predictive cooling system using fiber-optic temperature sensors and variable-speed pumps",
        "subComponents": null
      },
      {
        "name": "Health Monitoring Unit",
        "type": "Diagnostic Module",
        "description": "FPGA-based condition monitoring with RS-422 data bus integration",
        "subComponents": [
          {
            "name": "Vibration Analyzer",
            "description": "MEMS-based shock spectrum analysis for mechanical fault detection",
            "type": "Sensor Array"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Active Front-End Rectifier",
        "target": "Isolated DC-DC Stage",
        "relationshipType": "composition",
        "description": "DC link capacitors are shared between rectifier and DC-DC stage for energy buffering"
      },
      {
        "source": "Intelligent Thermal Manager",
        "target": "Isolated DC-DC Stage",
        "relationshipType": "dependency",
        "description": "Thermal management system requires real-time temperature data from converter's hotspot sensors"
      },
      {
        "source": "Health Monitoring Unit",
        "target": "Active Front-End Rectifier",
        "relationshipType": "aggregation",
        "description": "Collects switching frequency spectra for power device degradation analysis"
      },
      {
        "source": "EMI Supression Filter",
        "target": "SiC MOSFET Bridge",
        "relationshipType": "association",
        "description": "Filter parameters dynamically adjusted based on switching node ringing characteristics"
      }
    ],
    "attributes": [
      {
        "name": "inputVoltageRange",
