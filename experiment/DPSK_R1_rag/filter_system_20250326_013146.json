{
  "system_id": "filter_system",
  "system_name": "Filter System",
  "specification": {
    "designScenario": {
      "name": "Fuel Filtration System",
      "description": "A critical subsystem within the aircraft fuel system designed to remove particulate contaminants, water, and microbial growth from aviation fuel. It ensures engine protection by maintaining fuel purity across all flight phases while operating under variable pressure, temperature, and flow conditions. The system employs multi-stage filtration with fail-safe mechanisms to comply with SAE AS4059 and ICAO fuel cleanliness standards.",
      "requirements": [
        {
          "title": "Contaminant Removal",
          "type": "functional",
          "details": [
            {
              "description": "Must remove particles ≥10 microns with 99.9% efficiency and separate free water from fuel to prevent ice formation and microbial growth."
            }
          ]
        },
        {
          "title": "Pressure Drop Management",
          "type": "performance",
          "details": [
            {
              "description": "Maintain maximum differential pressure of 0.25 Bar at rated flow capacity (3000 L/hour) to prevent fuel starvation."
            }
          ]
        },
        {
          "title": "Bypass Safety",
          "type": "safety",
          "details": [
            {
              "description": "Automatic bypass valve activation at 0.35 Bar differential pressure to ensure uninterrupted fuel flow during filter clogging."
            }
          ]
        },
        {
          "title": "Maintainability",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Designed for 1000-hour service intervals with quick-disconnect filter elements meeting 5-minute replacement time criteria."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Primary Fuel Filter",
        "type": "Coarse Filtration Unit",
        "description": "First-stage stainless steel mesh filter capturing large particulates (>50 microns) and water droplets through centrifugal separation",
        "subComponents": [
          {
            "name": "Swirl Generator",
            "type": "Hydrodynamic Separator",
            "description": "Imparts rotational velocity to fuel for centrifugal water separation"
          },
          {
            "name": "Coalescer Media",
            "type": "Depth Filter",
            "description": "Glass fiber matrix combining particle capture and water coalescence"
          }
        ]
      },
      {
        "name": "Secondary Micronic Filter",
        "type": "Fine Filtration Unit",
        "description": "Beta-rated pleated cellulose/polyester composite filter for sub-micron particle removal (β₅≥200)",
        "subComponents": [
          {
            "name": "Filter Element",
            "type": "Replaceable Cartridge",
            "description": "ISO 2941-certified collapse-resistant media with end-of-life indicator"
          },
          {
            "name": "Drain Valve",
            "type": "Automatic Drain",
            "description": "Electro-pneumatic valve for accumulated water evacuation"
          }
        ]
      },
      {
        "name": "Differential Pressure Sensor",
        "type": "Monitoring Device",
        "description": "Redundant strain gauge sensors measuring pressure drop across filter stages",
        "subComponents": null
      },
      {
        "name": "Bypass Valve Assembly",
        "type": "Safety Mechanism",
        "description": "Spring-loaded poppet valve with manual override for emergency flow path selection",
        "subComponents": [
          {
            "name": "Actuator",
            "type": "Pressure-Responsive",
            "description": "Precision-calibrated Belleville washer stack for threshold pressure response"
          }
        ]
      },
      {
        "name": "Filter Health Monitoring Unit",
        "type": "Electronic Controller",
        "description": "Avionics-grade computer analyzing pressure trends and predicting filter lifespan",
        "subComponents": [
          {
            "name": "Condition Monitoring Module",
            "type": "Predictive Algorithm",
            "description": "Implements Weibull analysis for remaining useful life calculations"
          }
        ]
      }
    ],
    "relationships": [
      {
        "source": "Primary Fuel Filter",
        "target": "Secondary Micronic Filter",
        "relationshipType": "composition",
        "description": "Primary filter output is directly plumbed to secondary filter input through rigid fuel lines forming an integrated filtration stack"
      },
      {
        "source": "Differential Pressure Sensor",
        "target": "Filter Health Monitoring Unit",
        "relationshipType": "aggregation",
        "description": "Sensor array feeds real-time pressure data to the monitoring unit for system health analysis"
      },
      {
        "source": "Bypass Valve Assembly",
        "target": "Differential Pressure Sensor",
        "relationshipType": "dependency",
        "description": "Bypass valve activation logic depends on pressure differential measurements from redundant sensors"
      },
      {
        "source": "Filter Health Monitoring Unit",
        "target": "Fuel Control Module",
        "relationshipType": "association",
        "description": "Shares filter status data with the main fuel control system via ARINC 429 databus"
      },
      {
        "source": "Secondary Micronic Filter",
        "target": "Fuel Pump",
        "relationshipType": "dependency",
        "description": "Filter outlet connects directly to fuel pump inlet, requiring pressure stability to prevent pump cavitation"
      }
    ],
    "attributes": [
      {
        "name": "filtrationEfficiency",
        "type": "percentage",
        "value": "99.9% @ 15μm",
        "description": "Minimum particle capture efficiency at critical particle size per SAE AS4059 Class 3"
      },
      {
        "name": "maxPressureDrop",
        "type": "pressure",
        "value": "0.25 Bar",
        "description": "Allowed differential pressure across clean filter element at rated flow"
      },
      {
        "name": "flowCapacity",
        "type": "volumetric",
        "value": "3000 L/hour",
        "description": "Maximum sustained flow rate at 40°C fuel temperature"
      },
      {
        "name": "operatingTempRange",
        "type": "temperature",
        "value": "-40°C to +80°C",
        "description": "Qualified temperature envelope covering ground and flight operations"
      },
      {
        "name": "maintenanceInterval",
        "type": "temporal",
        "value": "1000 FH",
        "description": "Scheduled filter replacement interval under normal operating conditions"
      }
    ]
  },
