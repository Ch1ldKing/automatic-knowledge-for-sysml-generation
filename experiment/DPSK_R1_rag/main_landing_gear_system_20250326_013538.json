{
  "system_id": "main_landing_gear_system",
  "system_name": "Main Landing Gear System",
  "specification": {
    "designScenario": {
      "name": "Landing Gear System",
      "description": "Design a next-generation Landing Gear System for a commercial aircraft, integrating adaptive control algorithms, redundant electro-hydrostatic actuation (EHA), predictive health monitoring, and lightweight composite materials. The system must ensure operational reliability under extreme conditions (e.g., crosswind landings, contaminated runways) while enabling real-time diagnostics and energy-efficient performance.",
      "requirements": [
        {
          "title": "Adaptive Gear Deployment",
          "type": "functional",
          "details": [
            {
              "description": "The system must autonomously adjust deployment speed based on airspeed, altitude, and turbulence data to prevent structural overstress."
            },
            {
              "description": "Integrated obstacle detection sensors must prevent gear extension during ground proximity alerts."
            }
          ]
        },
        {
          "title": "Dynamic Load Management",
          "type": "performance",
          "details": [
            {
              "description": "Shock absorbers must dissipate 95% of vertical kinetic energy within 0.8 seconds during touchdown at 3.5 m/s sink rate."
            },
            {
              "description": "Electro-hydrostatic actuators shall achieve full gear extension in <2.5 seconds at -40°C ambient temperature."
            }
          ]
        },
        {
          "title": "Multi-Layer Redundancy",
          "type": "safety",
          "details": [
            {
              "description": "Triple-redundant control channels (electronic, fiber-optic, mechanical) must independently initiate emergency gear deployment."
            },
            {
              "description": "Passive gravity-assisted free-fall mechanism activates automatically if primary/secondary systems fail."
            }
          ]
        },
        {
          "title": "Lifecycle Optimization",
          "type": "nonFunctional",
          "details": [
            {
              "description": "Modular assembly design enabling component replacement in <45 minutes without full gear disassembly."
            },
            {
              "description": "Corrosion-resistant titanium alloys and CFRP composites meeting FAA 25.729 fatigue life requirements."
            }
          ]
        }
      ]
    },
    "systemComponents": [
      {
        "name": "Smart Actuation Controller",
        "type": "Avionics Module",
        "description": "AI-powered control unit with federated learning capabilities for optimizing gear dynamics based on historical operational data.",
        "subComponents": [
          {
            "name": "Neural Processing Unit",
            "description": "Dedicated hardware for real-time actuator trajectory optimization",
            "type": "Computational Core"
          },
          {
            "name": "Fault Detection Isolation Unit",
            "description": "Triple-voting comparator for critical command validation",
            "type": "Safety Processor"
          }
        ]
      },
      {
        "name": "Electro-Hydrostatic Actuation System",
        "type": "Power-by-Wire Subsystem",
        "description": "Self-contained hydraulic power units with regenerative energy recovery during gear retraction.",
        "subComponents": [
          {
            "name": "Compact Power Pack",
            "description": "10 kW brushless motor driving variable displacement piston pump",
            "type": "Electro-Hydraulic Actuator"
          },
          {
            "name": "Accumulator Module",
            "description": "Cellular silicone accumulator for peak power demand management",
            "type": "Energy Storage"
          }
        ]
      },
      {
        "name": "Morphing Shock Strut",
        "type": "Adaptive Structure",
        "description": "Magnetorheological fluid-based damper with variable stiffness tuning via electromagnetic field control.",
        "subComponents": [
          {
            "name": "MR Fluid Cartridge",
            "description": "Temperature-stable magnetorheological fluid matrix with 200 kPa yield stress",
            "type": "Smart Material"
          },
          {
            "name": "Flux Modulation Coil",
            "description": "High-frequency electromagnetic field generator for viscosity control",
            "type": "Actuation Interface"
          }
        ]
      },
      {
        "name": "Sensor Fusion Module",
        "type": "Distributed Sensing Network",
        "description": "Multi-physics sensor array measuring structural loads, vibration spectra, and hydraulic fluid condition.",
        "subComponents": [
          {
            "name": "Fiber Bragg Grating Array",
            "description": "Embedded optical strain sensors in critical load paths",
            "type": "Structural Health Monitor"
          },
          {
            "name": "Tribo-dynamic Analyzer",
            "description": "In-line fluid particulate counter with dielectric spectroscopy",
            "type": "Condition Monitor"
          }
        ]
      }
    ],
