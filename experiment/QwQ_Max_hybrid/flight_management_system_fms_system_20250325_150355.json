      },
      {
        "source": "Autopilot Interface",
        "target": "Central Flight Computer",
        "relationshipType": "composition",
        "description": "The Autopilot Interface is composed with the Central Flight Computer to execute control surface commands."
      },
      {
        "source": "Flight Data Recorder",
        "target": "Central Flight Computer",
        "relationshipType": "association",
        "description": "The Flight Data Recorder logs critical flight parameters from the Central Flight Computer."
      },
      {
        "source": "Sensor Fusion Hub",
        "target": "Multi-Mode Receiver",
        "relationshipType": "dependency",
        "description": "The Sensor Fusion Hub depends on GNSS data from the Multi-Mode Receiver for navigation calculations."
      }
    ],
